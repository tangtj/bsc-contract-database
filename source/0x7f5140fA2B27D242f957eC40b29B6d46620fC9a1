// File: project/plc/plc/hardhat/contracts/member.sol



contract Member {
    address public admin = msg.sender;
    
    mapping (string => address) public memberContract;

    modifier onlyOwner() {
        require(msg.sender == admin,"Not an administrator");
        _;
    }

    function setMember(string calldata name_,address addr_) public onlyOwner{
        memberContract[name_] = addr_;
    }

}
// File: project/plc/plc/hardhat/contracts/manage.sol






contract Manage {
    address public admin = msg.sender;
    mapping (address => bool) public whiteMap;
    
    address public member;

    modifier onlyOwner() {
        require(msg.sender == admin || whiteMap[msg.sender],"Not an administrator");
        _;
    }

    modifier onlyCreate() {
        require(msg.sender == admin,"Not an administrator");
        _;
    }

    function setWhite(address addr_,bool status_) external onlyCreate {
        whiteMap[addr_] = status_;
    }

    function setManage(address member_) public onlyOwner{
        member = member_;
    }

    function setOwner(address newOwner_) external onlyCreate {
        require(newOwner_ != address(0));
        admin = newOwner_;
    }
    
    function getAddress(string memory name_) public view returns(address) {
        address targetAddr =  Member(member).memberContract(name_);
        require(targetAddr != address(0),"error target address");
        return targetAddr;
    }

    function _decimals(address token_) public view returns(uint256){
        uint8 _decimal = IERC20Metadata(token_).decimals();
        return  10 ** uint256(_decimal);
    }

    function _getSuperior(address user_,uint256 plies_) public view returns(address){
        return ITeam(getAddress("team")).getSuperior(user_,plies_);
    }

    function _getGameActivate(address user_) public view returns(bool) {
        return ITeam(getAddress("team")).getGameActivate(user_);
    }

    function bugPrevention(address token_,uint256 amount_,address to) external onlyOwner {
        IERC20(token_).transfer(to,amount_);
    }
}
// File: project/plc/plc/hardhat/contracts/token.sol







// CPL代币合约地址：0x7f5140fA2B27D242f957eC40b29B6d46620fC9a1 代币已转入0x13d603b42f22B8537b627E8F123A830FAf031c00
// 1，必须先去pancakeswap内添加上面usdt和CPL的V2底池（一定是v2底池否则公排会报错）
// 2，必须将CPL代币打给这个合约地址（0xd10B4907c8137f11C920ca026F8806638708B501），这个地址一部分用作公排中奖后分发奖励额度，否则用户领取不到奖励
// 3，必须将CPL代币打给这个合约地址（0x7f5140fA2B27D242f957eC40b29B6d46620fC9a1）用作卖出时销毁50%，否则用户swap卖出时会报错

contract Token is ERC20 , Manage{

    IUniswapV2Router public router;

    address public pair;

    bool private closeFee;

    bool public openBuy;

    mapping (address => bool) public buyWhite;

    constructor(
        IUniswapV2Router _router,
        address _usdt
    ) ERC20('Coupling', 'CPL') {
        _mint(msg.sender,21000000 * (10 ** decimals()));
        router = _router;
        // need open
        pair = IUniswapV2Factory(router.factory()).createPair(address(this), _usdt);
        IERC20(pair).approve(address(router), type(uint256).max);
    }

    modifier closeFeeTransfer(){
        require(!closeFee,'in closeFee');
        closeFee = true;
        _;
        closeFee = false;
    }

    function burn(uint256 amount_) public {
        _burn(msg.sender,amount_);
    }

    function addLiquidity(
        address tokenA,
        address tokenB,
        uint256 amountADesired,
        uint256 amountBDesired,
        uint256 amountAMin,
        uint256 amountBMin,
        address to,
        uint256 deadline
    )
    external
    closeFeeTransfer
    returns (
        uint256 amountA,
        uint256 amountB,
        uint256 liquidity
    ){
        if(tokenA == address(this)){
            _transfer(msg.sender,address(this),amountADesired);
            _approve(address(this), address(router), amountADesired);
        }else{
            require(IERC20(tokenA).transferFrom(msg.sender,address(this),amountADesired),'A transferFrom error');
            IERC20(tokenA).approve(address(router), amountADesired);
        }

        if(tokenB == address(this)){
            _transfer(msg.sender,address(this),amountBDesired);
            _approve(address(this), address(router), amountBDesired);
        }else{
            require(IERC20(tokenB).transferFrom(msg.sender,address(this),amountBDesired),'B transferFrom error');
            IERC20(tokenB).approve(address(router), amountBDesired);
        }      
        
        (amountA,amountB,liquidity) = router.addLiquidity(tokenA, tokenB, amountADesired, amountBDesired, amountAMin, amountBMin, to, deadline);
        if(amountADesired - amountA > 0){
            IERC20(tokenA).transfer(msg.sender, amountADesired - amountA);
        }
        if(amountBDesired - amountB > 0){
            IERC20(tokenB).transfer(msg.sender, amountBDesired - amountB);
        }
    }

    function removeLiquidity(
        address tokenA,
        address tokenB,
        uint liquidity,
        uint amountAMin,
        uint amountBMin,
        address to,
        uint deadline
    ) external closeFeeTransfer returns (uint amountA, uint amountB) {
        require(IERC20(pair).transferFrom(msg.sender,address(this),liquidity),'pair transferFrom error');
        (amountA,amountB) = router.removeLiquidity(tokenA, tokenB, liquidity, amountAMin, amountBMin, to, deadline);
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override { 
        uint256 feeAmount;
        address _user = pair == to ? from : to;
        if(!buyWhite[_user]) {
            if ((pair == from && !closeFee) || (pair == to && !closeFee)) {
                if (pair == from) {
                    require(openBuy,'Prohibition of sale');
                }
                
                uint256 threePercent = amount * 3 / 100;
                uint256 twoPercent = amount * 2 / 100;
                uint256 onePercent = amount * 1 / 100;
                uint256 fivePartsPerThousand = amount * 5 / 1000; 
    
                super._transfer(from, getAddress("liquidity"), threePercent); 
                feeAmount += threePercent;
    
                address superior = _getSuperior(_user,1);

                if(superior != address(0) && _getGameActivate(superior)) {
                    super._transfer(from, superior, onePercent); 
                    feeAmount += onePercent;
                }
                    
                for (uint i = 2; i <= 5; i++) {
                    superior = _getSuperior(_user,i);
                    if(superior != address(0) && _getGameActivate(superior)) {
                        super._transfer(from, superior, fivePartsPerThousand);
                        feeAmount += fivePartsPerThousand;
                    }
                }

                _burn(from,onePercent);
                feeAmount += onePercent;

                super._transfer(from, getAddress("node"), twoPercent); 
                feeAmount += twoPercent;
                _nodeReward(twoPercent);

                super._transfer(from, getAddress("feeAddress"), onePercent);
                feeAmount += onePercent;
            }
            if (pair == to && !closeFee) {
                uint256 fiftyPercent = (amount - feeAmount) * 50 / 100;

                _burn(address(this),fiftyPercent);
                // feeAmount += fiftyPercent;
            }
        }
        super._transfer(from, to, amount - feeAmount);
    }

    function setOpenBuy(bool bol_) public onlyOwner {
        openBuy = bol_;
    }

    function setOpenBuyWhite(address adr_,bool bol_) public onlyOwner {
        buyWhite[adr_] = bol_;
    }

    function _nodeReward(uint256 amount) private {
        bytes memory data = abi.encodeWithSignature("addReceiveDividends(uint256)", amount);
        (bool result, ) = getAddress("node").call(data);
        require(result, "transaction failed");
    }
}