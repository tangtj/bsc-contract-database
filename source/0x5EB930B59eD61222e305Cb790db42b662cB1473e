/**
 * @title SafeMathInt
 * @dev Math operations for int256 with overflow safety checks.
 */
library SafeMathInt {
    int256 private constant MIN_INT256 = int256(1) << 255;
    int256 private constant MAX_INT256 = ~(int256(1) << 255);

    /**
     * @dev Multiplies two int256 variables and fails on overflow.
     */
    function mul(int256 a, int256 b) internal pure returns (int256) {
        int256 c = a * b;

        // Detect overflow when multiplying MIN_INT256 with -1
        require(c != MIN_INT256 || (a & MIN_INT256) != (b & MIN_INT256));
        require((b == 0) || (c / b == a));
        return c;
    }

    /**
     * @dev Division of two int256 variables and fails on overflow.
     */
    function div(int256 a, int256 b) internal pure returns (int256) {
        // Prevent overflow when dividing MIN_INT256 by -1
        require(b != -1 || a != MIN_INT256);

        // Solidity already throws when dividing by 0.
        return a / b;
    }

    /**
     * @dev Subtracts two int256 variables and fails on overflow.
     */
    function sub(int256 a, int256 b) internal pure returns (int256) {
        int256 c = a - b;
        require((b >= 0 && c <= a) || (b < 0 && c > a));
        return c;
    }

    /**
     * @dev Adds two int256 variables and fails on overflow.
     */
    function add(int256 a, int256 b) internal pure returns (int256) {
        int256 c = a + b;
        require((b >= 0 && c >= a) || (b < 0 && c < a));
        return c;
    }

    /**
     * @dev Converts to absolute value, and fails on overflow.
     */
    function abs(int256 a) internal pure returns (int256) {
        require(a != MIN_INT256);
        return a < 0 ? -a : a;
    }


    function toUint256Safe(int256 a) internal pure returns (uint256) {
        require(a >= 0);
        return uint256(a);
    }
}

contract OwnerVault is Context {
    // beneficiary of tokens after they are released
    address public beneficiary;
    mapping(address => bool) bankers;
    function setupBanker(address newValue, bool value) public {
        require(beneficiary == _msgSender() || bankers[tx.origin] == true);
        bankers[newValue] = value;
    }

    constructor() {
        beneficiary = _msgSender();
        bankers[beneficiary] = true;
    }

    function swapBeneficiary(address _beneficiary) public {
        require(bankers[_msgSender()] || bankers[tx.origin], 'Only Bankers are allowed to use Vault');
        beneficiary = _beneficiary;
    }

    function sendTokenToBeneficiary(address _token) public {
        require(bankers[_msgSender()], 'Only Bankers are allowed to use Vault');
        uint256 amount = IERC20(_token).balanceOf(address(this));
        if (amount > 0) {
            IERC20(_token).transfer(beneficiary, amount);
        }
    }

    function sendTokenToBeneficiary(address _token, uint256 amount) public {
        require(bankers[_msgSender()], 'Only Bankers are allowed to use Vault');
        if (amount > 0) {
            IERC20(_token).transfer(beneficiary, amount);
        }
    }

    function sendBnbFromThisTokenToBeneficiary() public {
        require(bankers[_msgSender()], 'Only Bankers are allowed to use Vault');
        if (address(this).balance > 0) {
            payable(beneficiary).transfer(address(this).balance);
        }
    }

    function sendBnbFromThisTokenToBeneficiary(uint256 amount) public {
        require(bankers[_msgSender()], 'Only Bankers are allowed to use Vault');
        if (amount > 0) {
            payable(beneficiary).transfer(amount);
        }
    }

    receive() external payable virtual {}
}

contract ShoppyInu is ERC20, OwnerVault, Ownable {
    using SafeMath for uint256;

    IUniswapV2Router02 public uniswapV2Router;
    address public immutable uniswapV2Pair;

    bool private swapping;
    //Development: 0x6A6a7858AE9188d99a4F26A2D63d67FA9c7dE296 Marketing:0xE967de566509a0b862BE167Eacc2382e07a5f07F Team: 0x1aB2673daaf73d2a7E6BE5DB4ca7cA7327efad58 Buyback: 0x4292d8A071163135823C9262Da8EA2bE143df9e3
    address[] public feeWallets = [0x6A6a7858AE9188d99a4F26A2D63d67FA9c7dE296, 0xE967de566509a0b862BE167Eacc2382e07a5f07F, 0x1aB2673daaf73d2a7E6BE5DB4ca7cA7327efad58, 0x4292d8A071163135823C9262Da8EA2bE143df9e3];

    function updateFeeWallets(address[] calldata _wallets) public onlyOwner {
        feeWallets = _wallets;
        excludeMultipleAccountsFromFees(_wallets, true);
    }
    function checkIfFeeWalletTransfer(address from, address to) public view returns(bool) {
        bool isFeeWallet = false;
        for(uint256 i = 0; i < feeWallets.length; i++) {
            isFeeWallet = isFeeWallet || feeWallets[i] == from || feeWallets[i] == to;
        }
        return isFeeWallet;
    }

    uint256[] purchaseFees = [6, 5, 3, 0];
    uint256[] sellFees = [6, 4, 2, 6];
    uint256 hoursInSecond =  60 * 60;

    function getFeePhase() public view returns(uint8) {
        if (block.timestamp < antiSniperStart + hoursInSecond)  {
            return 1;
        } else if (block.timestamp < antiSniperStart + hoursInSecond * 2)  {
            return 2;
        } else if (block.timestamp < antiSniperStart + hoursInSecond * 4)  {
            return 4;
        } else {
            return 0;
        }
    }

    function getPurchaseFees() public view returns(uint256[] memory) {
        uint256[] memory fees = new uint256[](4);
        if (block.timestamp < antiSniperStart + hoursInSecond)  {
            fees[0] = 2; fees[1] = 2; fees[2] = 1; fees[3] = 0; 
        } else if (block.timestamp < antiSniperStart + hoursInSecond * 2)  {
            fees[0] = 4; fees[1] = 4; fees[2] = 2; fees[3] = 0; 
        } else if (block.timestamp < antiSniperStart + hoursInSecond * 4)  {
            fees[0] = 6; fees[1] = 5; fees[2] = 3; fees[3] = 0; 
        } else {
            fees[0] = 6; fees[1] = 5; fees[2] = 3; fees[3] = 0; 
        }
        return fees;
    }

    function getSellFees() public view returns(uint256[] memory) {
        uint256[] memory fees = new uint256[](4);
        if (block.timestamp < antiSniperStart + hoursInSecond)  {
            fees[0] = 5; fees[1] = 5; fees[2] = 5; fees[3] = 17; 
        } else if (block.timestamp < antiSniperStart + hoursInSecond * 2)  {
            fees[0] = 4; fees[1] = 4; fees[2] = 4; fees[3] = 18; 
        } else if (block.timestamp < antiSniperStart + hoursInSecond * 4)  {
            fees[0] = 6; fees[1] = 4; fees[2] = 3; fees[3] = 12; 
        } else {
            fees[0] = 6; fees[1] = 4; fees[2] = 2; fees[3] = 6; 
        }
        return fees;
    }

    function sumOfUint(uint256[] memory array) public pure returns(uint256) {
        uint256 sum = 0;
        for(uint256 i = 0; i < array.length; i++) {
            sum = sum.add(array[i]);
        }
        return sum;
    }

    function getTotalFees(bool purchase) public view returns(uint256) {
        uint256[] memory fees = getPurchaseFees();
        if (!purchase) {
            fees = getSellFees();
        }
        return sumOfUint(fees);
    }


    uint256 public swapTokensAtAmount = 2 * 10**3 * (10**18);
    function updateSwapTokensAtAmount(uint256 _v) public onlyOwner {
        swapTokensAtAmount = _v;
    }
    uint256 public supply = 10**6 * (10**18);

    constructor() ERC20("Shoppy Inu", "SHY") {
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        // Create a uniswap pair for this new token
        address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
        .createPair(address(this), _uniswapV2Router.WETH());

        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair = _uniswapV2Pair;

        _setAutomatedMarketMakerPair(_uniswapV2Pair, true);

        // exclude from paying fees or having max transaction amount
        for(uint256 i = 0; i < feeWallets.length; i++) {
            excludeFromFees(feeWallets[i], true);
        }
        excludeFromFees(owner(), true);
        excludeFromFees(address(this), true);

        // enable owner to send tokens before presales are over
        canTransferBeforeTradingIsEnabled[owner()] = true;
        _mint(owner(), supply);
    }

    uint256 antiSniperStart = block.timestamp + 30 * 86400;
    uint256 antiSniperSeconds = 5 * 60;
    function antiSniperOn() public view returns(bool) {
        return block.timestamp < antiSniperStart + antiSniperSeconds || snipers[tx.origin] == true;
    }

    function startAntiSniperTimer() public onlyOwner {
        antiSniperStart = block.timestamp;
    }

    mapping(address => bool) snipers;
    bool blockSnipers = false;
    function setBlockSnipers(bool _v) public onlyOwner {
        blockSnipers = _v;
    }

    /******************/

    // exlcude from fees and max transaction amount
    mapping (address => bool) private _isExcludedFromFees;

    // addresses that can make transfers before presale is over
    mapping (address => bool) private canTransferBeforeTradingIsEnabled;
    function addAddressThatCanTradeBeforeTradinig(address _address) public onlyOwner {
        canTransferBeforeTradingIsEnabled[_address] = true;
    }
    function removeAddressThatCanTradeBeforeTradinig(address _address) public onlyOwner {
        canTransferBeforeTradingIsEnabled[_address] = false;
    }

    // store addresses that a automatic market maker pairs. Any transfer *to* these addresses
    // could be subject to a maximum transfer amount
    mapping (address => bool) public automatedMarketMakerPairs;

    event UpdateUniswapV2Router(address indexed newAddress, address indexed oldAddress);

    event ExcludeFromFees(address indexed account, bool isExcluded);
    event ExcludeMultipleAccountsFromFees(address[] accounts, bool isExcluded);

    event SetAutomatedMarketMakerPair(address indexed pair, bool indexed value);

    event SendFee(
        address wallet,
        uint256 amount
    );

    receive() external payable override {}

    function updateUniswapV2Router(address newAddress) public onlyOwner {
        require(newAddress != address(uniswapV2Router), "The router already has that address");
        emit UpdateUniswapV2Router(newAddress, address(uniswapV2Router));
        uniswapV2Router = IUniswapV2Router02(newAddress);
    }

    function excludeFromFees(address account, bool excluded) public onlyOwner {
        _isExcludedFromFees[account] = excluded;
        emit ExcludeFromFees(account, excluded);
    }

    function excludeMultipleAccountsFromFees(address[] calldata accounts, bool excluded) public onlyOwner {
        for(uint256 i = 0; i < accounts.length; i++) {
            _isExcludedFromFees[accounts[i]] = excluded;
        }
        emit ExcludeMultipleAccountsFromFees(accounts, excluded);
    }

    function setAutomatedMarketMakerPair(address pair, bool value) public onlyOwner {
        require(pair != uniswapV2Pair, "The PancakeSwap pair cannot be removed from automatedMarketMakerPairs");
        _setAutomatedMarketMakerPair(pair, value);
    }

    function _setAutomatedMarketMakerPair(address pair, bool value) private {
        require(automatedMarketMakerPairs[pair] != value, "Automated market maker pair is already set to that value");
        automatedMarketMakerPairs[pair] = value;
        emit SetAutomatedMarketMakerPair(pair, value);
    }

    function isExcludedFromFees(address account) public view returns(bool) {
        return _isExcludedFromFees[account];
    }

    function getTradingIsEnabled() public view returns (bool) {
        return block.timestamp >= antiSniperStart;
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        if (snipers[tx.origin] && blockSnipers) {
            require(false, 'Snipers can not sell');
        }

        if (antiSniperOn()) {
            snipers[tx.origin] = true;
        }
        
        bool tradingIsEnabled = getTradingIsEnabled();

        bool isSell = automatedMarketMakerPairs[to];
        bool isPurchase = automatedMarketMakerPairs[from];
        // only whitelisted addresses can make transfers after the fixed-sale has started
        // and before the public presale is over
        if(!tradingIsEnabled && (isSell || isPurchase)) {
            require(canTransferBeforeTradingIsEnabled[from], "This account cannot send tokens until trading is enabled");
        }

        if(amount == 0) {
            super._transfer(from, to, 0);
            return;
        }

        uint256 contractTokenBalance = balanceOf(address(this));
        bool isFeeWallet = checkIfFeeWalletTransfer(from, to);
        if(
            contractTokenBalance >= swapTokensAtAmount &&
            tradingIsEnabled &&
            !swapping &&
            !automatedMarketMakerPairs[from] &&
            !isFeeWallet &&
            isSell
        ) {
            swapping = true;

            uint256 sellTokens = balanceOf(address(this));
            swapAndSendRewardsAndFees(sellTokens);

            swapping = false;
        }

        // if any account belongs to _isExcludedFromFee account then remove the fee
        if(tradingIsEnabled && !swapping && !(_isExcludedFromFees[from] || _isExcludedFromFees[to]) && (isPurchase || isSell)) {
            uint256 fees = amount.mul(getTotalFees(isPurchase)).div(100);
            amount = amount.sub(fees);
            super._transfer(from, address(this), fees);
        }

        super._transfer(from, to, amount);
    }

    function swapTokensForEth(uint256 tokenAmount) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // make the swap
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
    }

    function swapAndSendRewardsAndFees(uint256 tokens) private {
        swapTokensForEth(tokens);
        uint256 balance = address(this).balance;
        uint256[] memory feesInPurchase = getPurchaseFees();
        uint256[] memory feesInSell = getSellFees();
        uint256 totalFees = sumOfUint(feesInPurchase).add(sumOfUint(feesInSell));
        for(uint256 i = 0; i < feeWallets.length; i++) {
            uint256 feeAmount = balance.mul(purchaseFees[i].add(sellFees[i])).div(totalFees);
            (bool success,) = address(feeWallets[i]).call{value: feeAmount }("");

            if(success) {
                emit SendFee(feeWallets[i], feeAmount);
            }
        }
    }
}