pragma solidity >= 0.7.6;

interface IEMERouter {
    function factory() external view returns (address);
    function WETH() external view returns (address);

    function addLiquidity(
        address tokenA,
        address tokenB,
        uint amountADesired,
        uint amountBDesired,
        uint amountAMin,
        uint amountBMin,
        address to,
        uint deadline
    ) external returns (uint amountA, uint amountB, uint liquidity);
    function addLiquidityETH(
        address token,
        uint amountTokenDesired,
        uint amountTokenMin,
        uint amountETHMin,
        address to,
        uint deadline
    ) external payable returns (uint amountToken, uint amountETH, uint liquidity);
    function removeLiquidity(
        address tokenA,
        address tokenB,
        uint liquidity,
        uint amountAMin,
        uint amountBMin,
        address to,
        uint deadline
    ) external returns (uint amountA, uint amountB);
    function removeLiquidityETH(
        address token,
        uint liquidity,
        uint amountTokenMin,
        uint amountETHMin,
        address to,
        uint deadline
    ) external returns (uint amountToken, uint amountETH);
    function removeLiquidityWithPermit(
        address tokenA,
        address tokenB,
        uint liquidity,
        uint amountAMin,
        uint amountBMin,
        address to,
        uint deadline,
        bool approveMax, uint8 v, bytes32 r, bytes32 s
    ) external returns (uint amountA, uint amountB);
    function removeLiquidityETHWithPermit(
        address token,
        uint liquidity,
        uint amountTokenMin,
        uint amountETHMin,
        address to,
        uint deadline,
        bool approveMax, uint8 v, bytes32 r, bytes32 s
    ) external returns (uint amountToken, uint amountETH);
    function swapExactTokensForTokens(
        uint amountIn,
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external returns (uint[] memory amounts);
    function swapTokensForExactTokens(
        uint amountOut,
        uint amountInMax,
        address[] calldata path,
        address to,
        uint deadline
    ) external returns (uint[] memory amounts);
    function swapExactETHForTokens(uint amountOutMin, address[] calldata path, address to, uint deadline)
        external
        payable
        returns (uint[] memory amounts);
    function swapTokensForExactETH(uint amountOut, uint amountInMax, address[] calldata path, address to, uint deadline)
        external
        returns (uint[] memory amounts);
    function swapExactTokensForETH(uint amountIn, uint amountOutMin, address[] calldata path, address to, uint deadline)
        external
        returns (uint[] memory amounts);
    function swapETHForExactTokens(uint amountOut, address[] calldata path, address to, uint deadline)
        external
        payable
        returns (uint[] memory amounts);

    function quote(uint amountA, uint reserveA, uint reserveB) external pure returns (uint amountB);
    function getAmountOut(uint amountIn, uint reserveIn, uint reserveOut) external pure returns (uint amountOut);
    function getAmountIn(uint amountOut, uint reserveIn, uint reserveOut) external pure returns (uint amountIn);
    function getAmountsOut(uint amountIn, address[] calldata path) external view returns (uint[] memory amounts);
    function getAmountsIn(uint amountOut, address[] calldata path) external view returns (uint[] memory amounts);

    function removeLiquidityETHSupportingFeeOnTransferTokens(
        address token,
        uint liquidity,
        uint amountTokenMin,
        uint amountETHMin,
        address to,
        uint deadline
    ) external returns (uint amountETH);
    function removeLiquidityETHWithPermitSupportingFeeOnTransferTokens(
        address token,
        uint liquidity,
        uint amountTokenMin,
        uint amountETHMin,
        address to,
        uint deadline,
        bool approveMax, uint8 v, bytes32 r, bytes32 s
    ) external returns (uint amountETH);

    function swapExactTokensForTokensSupportingFeeOnTransferTokens(
        uint amountIn,
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external;
    function swapExactETHForTokensSupportingFeeOnTransferTokens(
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external payable;
    function swapExactTokensForETHSupportingFeeOnTransferTokens(
        uint amountIn,
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external;
}


// File: contracts/pool/RouterReward.sol
// SPDX-License-Identifier: MIT

pragma solidity >=0.7.6;
pragma abicoder v2;

import "../interfaces/IEMERouter.sol";
import "./SmartDisPatchInitializable.sol";
import "../interfaces/IEMEPair.sol";
import "../interfaces/IEMEFactory.sol";
import "../core/SafeOwnable.sol";
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";
import "@openzeppelin/contracts/math/SafeMath.sol";
import "../tool/ReentrancyGuard.sol";

contract RouterReward is SafeOwnable, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using Address for address;

    struct BounsInfo {
        address pool;
        uint256 bonus;
    }

    struct UserInfo {
        address account;
        uint8 level;
    }

    IERC20 public TOKEN;
    IERC20 public emeSwapV2Pair;
    address public liquUitlReward;

    address public constant hole = 0x000000000000000000000000000000000000dEaD;
    mapping(uint8 => uint256) public levelRate;
    mapping(address => address) public userMap;
    mapping(address => bool) public validUser;
    mapping(address => uint8) public userLevel;
    mapping(address => uint256) public aleaReward;
    mapping(address => uint256) public userActive;
    mapping(address => address[]) public users;
    mapping(address => uint256) public teamUsdt;
    mapping(address => mapping(uint8 => uint256)) public userLevelCount;

    uint256 private _totalSupply;
    uint256 public rateTotal;
    uint256 public createPoolAmounts;
    mapping(address => uint256) private _balances;
    mapping(uint8 => address) public smartDisPatch;

    event RewardAdded(uint256 reward);
    event Staked(address indexed user, uint256 amount);
    event Withdrawn(address indexed user, uint256 amount);
    event RewardPaid(address indexed user, uint256 reward);
    event CreatePool(address v1, address v2, address v3, address v4);

    bool automatic;
    uint256 public manualAmount;

    address public fundAddr;
    address public router;
    address public treasury;

    /** transfer fee */
    uint256 public holeFee = 15;
    uint256 public fundFee = 15;
    uint256 public bounsFee = 70;

    /** bouns fee */
    uint256 public addLiquidityFee = 35;
    uint256 public v1Fee = 35;
    uint256 public v2Fee = 17;
    uint256 public v3Fee = 8;
    uint256 public v4Fee = 5;

    constructor(
        address _token,
        address _router,
        address _pair,
        address _treasury,
        address _fundAddr
    ) {
        TOKEN = IERC20(_token);
        router = _router;
        userMap[_treasury] = _treasury;
        fundAddr = _fundAddr;
        treasury = _treasury;
        emeSwapV2Pair = IERC20(_pair);

        levelRate[0] = 40;
        levelRate[1] = 60;
        levelRate[2] = 80;
        levelRate[3] = 90;
        levelRate[4] = 100;
        rateTotal = 100;
    }

    receive() external payable {}

    function getPair() public view returns (address) {
        return address(emeSwapV2Pair);
    }

    function updateTreasury(address _treasury) external onlyOwner {
        treasury = _treasury;
    }

    function updateFundAddr(address _fundAddr) external onlyOwner {
        fundAddr = _fundAddr;
    }

    function updateLevelRate(
        uint256 _v0,
        uint256 _v1,
        uint256 _v2,
        uint256 _v3,
        uint256 _v4
    ) external onlyOwner {
        levelRate[0] = _v0;
        levelRate[1] = _v1;
        levelRate[2] = _v2;
        levelRate[3] = _v3;
        levelRate[4] = _v4;
    }

    function updateTransferFee(
        uint256 _hole,
        uint256 _fund,
        uint256 _bonus
    ) external onlyOwner {
        require(_hole.add(_fund).add(_bonus) <= 100, "rate max 100");
        holeFee = _hole;
        fundFee = _fund;
        bounsFee = _bonus;
    }

    function openAutomatic() external onlyOwner {
        if (automatic) {
            automatic = false;
        } else {
            automatic = true;
        }
    }

    function updateBonusFee(
        uint256 _addLiquidityFee,
        uint256 _v1Fee,
        uint256 _v2Fee,
        uint256 _v3Fee,
        uint256 _v4Fee
    ) external onlyOwner {
        require(
            _addLiquidityFee.add(_v1Fee).add(_v2Fee).add(_v3Fee).add(_v4Fee) <=
                100,
            "not rate"
        );
        addLiquidityFee = _addLiquidityFee;
        v1Fee = _v1Fee;
        v2Fee = _v2Fee;
        v3Fee = _v3Fee;
        v4Fee = _v4Fee;
    }

    function createPoolInitialize() external onlyOwner {
        address v1 = createDivPool(address(TOKEN));
        smartDisPatch[1] = v1;
        address v2 = createDivPool(address(TOKEN));
        smartDisPatch[2] = v2;
        address v3 = createDivPool(address(TOKEN));
        smartDisPatch[3] = v3;
        address v4 = createDivPool(address(TOKEN));
        smartDisPatch[4] = v4;
        liquUitlReward = createDivPool(address(TOKEN));
        emit CreatePool(v1, v2, v3, v4);
    }

    function createDivPool(address _token) internal returns (address) {
        createPoolAmounts++;
        bytes memory bytecode = type(SmartDisPatchInitializable).creationCode;
        bytes32 salt = keccak256(
            abi.encodePacked(address(this), createPoolAmounts)
        );
        address poolAddress;
        assembly {
            poolAddress := create2(0, add(bytecode, 32), mload(bytecode), salt)
        }
        SmartDisPatchInitializable(poolAddress).initialize(_token, treasury);
        return poolAddress;
    }

    function setInviter(address _inviter) public {
        require(userMap[_inviter] != address(0), "not inviter");
        require(userMap[msg.sender] == address(0), "not inviter");
        require(msg.sender != _inviter, "not inviter");
        users[_inviter].push(msg.sender);
        userMap[msg.sender] = _inviter;
    }

    function downgrade(address _inviter) private {
        uint8 invLevel = userLevel[_inviter];

        if (
            invLevel == 1 &&
            (teamUsdt[_inviter] < v1Amount ||
                userLevelCount[_inviter][0] < v1Count)
        ) {
            userLevel[_inviter] = invLevel - 1;
        } else if (invLevel == 2 && userLevelCount[_inviter][1] < 3) {
            userLevel[_inviter] = invLevel - 1;
        } else if (invLevel == 3 && userLevelCount[_inviter][2] < 3) {
            userLevel[_inviter] = invLevel - 1;
        } else if (invLevel == 4 && userLevelCount[_inviter][3] < 3) {
            userLevel[_inviter] = invLevel - 1;
        } else return;
        if (invLevel != 0 && validUser[_inviter]) {
            SmartDisPatchInitializable(smartDisPatch[invLevel]).withdraw(
                _inviter,
                1
            );
        }
        if (invLevel - 1 >= 1 && validUser[_inviter]) {
            SmartDisPatchInitializable(smartDisPatch[invLevel - 1]).stake(
                _inviter,
                1
            );
        }

        if (userLevelCount[_inviter][invLevel] >= 1) return;

        address user = userMap[_inviter];
        if (userLevelCount[user][invLevel] <= 0) return;

        userLevelCount[user][invLevel] = userLevelCount[user][invLevel].sub(1);

        address inv = userMap[user];
        for (int256 i = 0; i != 18; i++) {
            if (inv == address(0) || inv == user) {
                break;
            }
            if (userLevel[user] >= invLevel) {
                break;
            }
            if (userLevelCount[inv][invLevel] <= 0) {
                break;
            }
            userLevelCount[inv][invLevel] = userLevelCount[inv][invLevel].sub(
                1
            );
            user = inv;
            inv = userMap[inv];
        }
    }

    uint256 public v1Count = 3;
    uint256 public v1Amount = 26000 * 10**18;

    function updateV1Config(uint256 _v1Count, uint256 _v1Amount)
        external
        onlyOwner
    {
        v1Count = _v1Count;
        v1Amount = _v1Amount;
    }

    function upgrade(address _inviter) private {
        uint8 invLevel = userLevel[_inviter];
        if (
            invLevel == 0 &&
            userLevelCount[_inviter][0] >= v1Count &&
            teamUsdt[_inviter] >= v1Amount
        ) {
            userLevel[_inviter] = invLevel + 1;
        } else if (invLevel == 1 && userLevelCount[_inviter][1] >= 3) {
            userLevel[_inviter] = invLevel + 1;
        } else if (invLevel == 2 && userLevelCount[_inviter][2] >= 3) {
            userLevel[_inviter] = invLevel + 1;
        } else if (invLevel == 3 && userLevelCount[_inviter][3] >= 3) {
            userLevel[_inviter] = invLevel + 1;
        } else return;

        if (invLevel != 0 && validUser[_inviter]) {
            SmartDisPatchInitializable(smartDisPatch[invLevel]).withdraw(
                _inviter,
                1
            );
        }
        invLevel = invLevel + 1;
        if (validUser[_inviter]) {
            SmartDisPatchInitializable(smartDisPatch[invLevel]).stake(
                _inviter,
                1
            );
        }

        if (userLevelCount[_inviter][invLevel] >= 1) return;

        address user = userMap[_inviter];
        userLevelCount[user][invLevel] = userLevelCount[user][invLevel].add(1);

        address inv = userMap[user];
        for (int256 i = 0; i != 18; i++) {
            if (inv == address(0) || inv == user) {
                break;
            }
            if (userLevel[user] >= invLevel) {
                break;
            }
            userLevelCount[inv][invLevel] = userLevelCount[inv][invLevel].add(
                1
            );
            user = inv;
            inv = userMap[inv];
        }
    }

    uint256 public v0Amount = 170 * 10**18;

    function v1FeeConfig(uint256 _v0Amount) external onlyOwner {
        require(_v0Amount > v0Amount, "not v0Amount");
        v0Amount = _v0Amount;
    }

    function stakeFor(
        address account,
        uint256 liquidity,
        uint256 fee
    ) public nonReentrant {
        require(router == msg.sender, "not router");
        require(userMap[account] != address(0), "not inviter");
        _totalSupply = _totalSupply.add(liquidity);
        _balances[account] = _balances[account].add(liquidity);
        SmartDisPatchInitializable(liquUitlReward).stake(account, liquidity);

        emeSwapV2Pair.safeTransferFrom(msg.sender, address(this), fee);

        calculateInvReward(account, fee);

        if (!validUser[account] && _balances[account] >= v0Amount) {
            userLevelCount[userMap[account]][0] = userLevelCount[
                userMap[account]
            ][0].add(1);

            uint8 level = userLevel[account];
            if (level > 0) {
                SmartDisPatchInitializable(smartDisPatch[level]).stake(
                    account,
                    1
                );
            }
            validUser[account] = true;
        }

        updateTeamAmount(account, liquidity, true);
        updateStakeTeam(account);
        emit Staked(account, liquidity);
    }

    function withdrawFor(address account, uint256 liquidity)
        public
        nonReentrant
    {
        require(router == msg.sender, "not router");
        _totalSupply = _totalSupply.sub(liquidity);
        _balances[account] = _balances[account].sub(liquidity);
        SmartDisPatchInitializable(liquUitlReward).withdraw(account, liquidity);

        if (validUser[account] && _balances[account] < v0Amount) {
            userLevelCount[userMap[account]][0] = userLevelCount[
                userMap[account]
            ][0].sub(1);

            uint8 level = userLevel[account];
            if (level > 0) {
                SmartDisPatchInitializable(smartDisPatch[level]).withdraw(
                    account,
                    1
                );
            }
            validUser[account] = false;
        }

        updateTeamAmount(account, liquidity, false);
        updateWithdrawTeam(account);
        emit Withdrawn(account, liquidity);
    }

    function updateTeamAmount(
        address _account,
        uint256 _team,
        bool isAdd
    ) private {
        address _inviter = userMap[_account];
        address que;
        for (uint256 index = 0; index != 3; index++) {
            if (_inviter == address(0) || que == _inviter) {
                break;
            }
            if (isAdd) {
                teamUsdt[_inviter] = teamUsdt[_inviter].add(_team);
            } else {
                teamUsdt[_inviter] = teamUsdt[_inviter].sub(_team);
            }
            que = _inviter;
            _inviter = userMap[_inviter];
        }
    }

    function updateStakeTeam(address _account) private {
        address _inviter = userMap[_account];
        address que;
        for (uint256 i = 0; i != 20; i++) {
            if (_inviter == address(0) || que == _inviter) {
                break;
            }
            upgrade(_inviter);
            que = _inviter;
            _inviter = userMap[_inviter];
        }
    }

    function updateWithdrawTeam(address _inviter) private {
        address que;
        for (uint256 i = 0; i != 20; i++) {
            if (_inviter == address(0) || que == _inviter) {
                break;
            }
            downgrade(_inviter);
            que = _inviter;
            _inviter = userMap[_inviter];
        }
    }

    function balanceOf(address account) public view returns (uint256) {
        return _balances[account];
    }

    function calculateInvReward(address _sender, uint256 reward) private {
        address inviterAddr = userMap[_sender];
        if (inviterAddr == address(0)) return;
        uint256 power = balanceOf(_sender);
        uint256 _rateTotal;
        if (validUser[inviterAddr]) {
            uint256 activeRate = levelRate[userLevel[inviterAddr]];
            //superier reward
            if (balanceOf(inviterAddr) < power) {
                uint256 value = balanceOf(inviterAddr)
                    .mul(10**18)
                    .div(power)
                    .mul(reward)
                    .mul(activeRate)
                    .div(100);
                userActive[inviterAddr] = userActive[inviterAddr].add(
                    value.div(10**18)
                );
                emeSwapV2Pair.transfer(
                    treasury,
                    reward.mul(activeRate).div(100).sub(value.div(10**18))
                );
            } else {
                userActive[inviterAddr] = userActive[inviterAddr].add(
                    reward.mul(activeRate).div(100)
                );
            }
            _rateTotal = activeRate;
        }
        //team reward
        uint8 levelCou = 1;
        for (uint256 i = 0; i != 100; i++) {
            inviterAddr = userMap[inviterAddr];
            if (inviterAddr == address(0)) {
                break;
            }
            if (balanceOf(inviterAddr) <= 0) {
                continue;
            }
            if (!validUser[inviterAddr]) {
                continue;
            }

            uint8 invLevel = userLevel[inviterAddr];
            if (_rateTotal < rateTotal && invLevel >= levelCou) {
                uint256 _rate = 0;
                if (levelRate[invLevel] > _rateTotal) {
                    _rate = levelRate[invLevel].sub(_rateTotal);
                }
                if (_rate > 0) {
                    _rateTotal = _rateTotal.add(_rate);
                    userActive[inviterAddr] = userActive[inviterAddr].add(
                        reward.mul(_rate).div(100)
                    );
                }
                levelCou = invLevel + 1;
            }
            if (levelCou > 4) {
                break;
            }
        }
        if (_rateTotal < rateTotal) {
            emeSwapV2Pair.transfer(
                treasury,
                reward.mul(rateTotal.sub(_rateTotal)).div(100)
            );
        }
    }

    function getActiveReward() public nonReentrant {
        uint256 active = userActive[msg.sender];
        require(active > 0, "not active reward");
        userActive[msg.sender] = 0;

        emeSwapV2Pair.transfer(msg.sender, active);
        aleaReward[msg.sender] = aleaReward[msg.sender].add(active);
    }

    function donate(uint256 reward) external nonReentrant {
        if (holeFee > 0) {
            TOKEN.safeTransferFrom(
                msg.sender,
                hole,
                reward.mul(holeFee).div(100)
            );
        }
        if (fundFee > 0) {
            TOKEN.safeTransferFrom(
                msg.sender,
                fundAddr,
                reward.mul(fundFee).div(100)
            );
        }

        uint256 bouns = reward.mul(bounsFee).div(100);

        if (addLiquidityFee > 0) {
            TOKEN.safeTransferFrom(
                msg.sender,
                liquUitlReward,
                bouns.mul(addLiquidityFee).div(100)
            );
        }
        if (v1Fee > 0) {
            TOKEN.safeTransferFrom(
                msg.sender,
                smartDisPatch[1],
                bouns.mul(v1Fee).div(100)
            );
        }
        if (v2Fee > 0) {
            TOKEN.safeTransferFrom(
                msg.sender,
                smartDisPatch[2],
                bouns.mul(v2Fee).div(100)
            );
        }
        if (v3Fee > 0) {
            TOKEN.safeTransferFrom(
                msg.sender,
                smartDisPatch[3],
                bouns.mul(v3Fee).div(100)
            );
        }
        if (v4Fee > 0) {
            TOKEN.safeTransferFrom(
                msg.sender,
                smartDisPatch[4],
                bouns.mul(v4Fee).div(100)
            );
        }
    }

    function getUserBonus(address account)
        public
        view
        returns (BounsInfo[] memory bonusInfo)
    {
        bonusInfo = new BounsInfo[](4);
        uint256 v1 = SmartDisPatchInitializable(smartDisPatch[1]).available(
            account
        );
        bonusInfo[0].pool = smartDisPatch[1];
        bonusInfo[0].bonus = v1;
        uint256 v2 = SmartDisPatchInitializable(smartDisPatch[2]).available(
            account
        );
        bonusInfo[1].pool = smartDisPatch[2];
        bonusInfo[1].bonus = v2;
        uint256 v3 = SmartDisPatchInitializable(smartDisPatch[3]).available(
            account
        );
        bonusInfo[2].pool = smartDisPatch[3];
        bonusInfo[2].bonus = v3;
        uint256 v4 = SmartDisPatchInitializable(smartDisPatch[4]).available(
            account
        );
        bonusInfo[3].pool = smartDisPatch[4];
        bonusInfo[3].bonus = v4;
    }

    function inviteInfo(
        address _account,
        uint256 page,
        uint256 size
    ) public view returns (UserInfo[] memory _users) {
        _users = new UserInfo[](size);
        if (page > 0) {
            uint256 startIndex = page.sub(1).mul(size);
            address[] memory inviters = users[_account];
            uint256 length = inviters.length;
            for (uint256 i = 0; i < size; i++) {
                if (startIndex.add(i) >= length) {
                    break;
                }
                _users[i].account = inviters[startIndex.add(i)];
                uint8 level = userLevel[inviters[startIndex.add(i)]];
                if (!validUser[inviters[startIndex.add(i)]]) {
                    _users[i].level = level;
                } else {
                    _users[i].level = level + 1;
                }
            }
        }
    }

    function getLiquReward(address account)
        public
        view
        returns (
            address _contract,
            uint256 _balance,
            uint256 _reward
        )
    {
        _contract = liquUitlReward;
        _balance = SmartDisPatchInitializable(liquUitlReward).balanceOf(
            account
        );
        _reward = SmartDisPatchInitializable(liquUitlReward).available(account);
    }
}


// File: contracts/pool/SmartDisPatchInitializable.sol
// SPDX-License-Identifier: MIT

pragma solidity >=0.7.6;
pragma abicoder v2;
import "@openzeppelin/contracts/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/proxy/Initializable.sol";

import "hardhat/console.sol";

contract SmartDisPatchInitializable is Ownable, Initializable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    // The address of the smart chef factory
    address public SMART_DISPATCH_FACTORY;

    uint256 private reserve;
    uint256 private lastTime;
    uint256 private rewardLastStored;
    uint256 private _totalSupply;
    address public vault;
    mapping(address => uint256) private _balances;
    mapping(address => uint256) private userRewardStored;
    mapping(address => uint256) private newReward;

    IERC20 public rewardToken;
    mapping(address => uint256) public rewardPair;

    event Staked(address indexed user, uint256 amount);
    event Withdrawn(address indexed user, uint256 amount);
    event RewardPaid(address indexed user, uint256 reward);

    constructor() {
        SMART_DISPATCH_FACTORY = msg.sender;
    }

    function initialize(address rewardToken_, address _vault)
        external
        initializer
    {
        require(msg.sender == SMART_DISPATCH_FACTORY, "Not factory");
        vault = _vault;
        rewardToken = IERC20(rewardToken_);
        transferOwnership(msg.sender);
    }

    function setVault(address _newVault) external {
        require(msg.sender == vault, "caller not is vault");
        vault = _newVault;
    }

    modifier updateDispatch(address account) {
        rewardLastStored = rewardPer();

        uint256 balance = rewardToken.balanceOf(address(this));
        if (_totalSupply == 0 && balance > 0) {
            rewardToken.safeTransfer(vault, balance);
            balance = 0;
        }
        reserve = balance;

        if (account != address(0)) {
            newReward[account] = available(account);
            userRewardStored[account] = rewardLastStored;
        }
        _;
    }

    function lastReward() private view returns (uint256) {
        if (_totalSupply == 0) {
            return 0;
        }
        uint256 balance = rewardToken.balanceOf(address(this));
        return balance.sub(reserve);
    }

    function totalSupply() public view returns (uint256) {
        return _totalSupply;
    }

    function balanceOf(address account) public view returns (uint256) {
        return _balances[account];
    }

    function rewardPer() private view returns (uint256) {
        if (totalSupply() == 0) {
            return rewardLastStored;
        }
        return rewardLastStored.add(lastReward().mul(1e18).div(totalSupply()));
    }

    function stake(address account, uint256 amount)
        external
        updateDispatch(account)
        onlyOwner
    {
        _totalSupply = _totalSupply.add(amount);
        _balances[account] = _balances[account].add(amount);
        emit Staked(account, amount);
    }

    function withdraw(address account, uint256 amount)
        external
        updateDispatch(account)
        onlyOwner
    {
        _totalSupply = _totalSupply.sub(amount);
        _balances[account] = _balances[account].sub(amount);
        emit Withdrawn(account, amount);
    }

    function available(address account) public view returns (uint256) {
        return
            balanceOf(account)
                .mul(rewardPer().sub(userRewardStored[account]))
                .div(1e18)
                .add(newReward[account]);
    }

    function claim() external updateDispatch(msg.sender) {
        uint256 reward = available(msg.sender);
        if (reward <= 0) {
            return;
        }
        reserve = reserve.sub(reward);
        newReward[msg.sender] = 0;

        rewardPair[msg.sender] = rewardPair[msg.sender].add(reward);

        rewardToken.safeTransfer(msg.sender, reward);
        emit RewardPaid(msg.sender, reward);
    }

    function sync() public updateDispatch(address(0)) {}
}


// File: contracts/tool/ReentrancyGuard.sol
// SPDX-License-Identifier: MIT


// SPDX-License-Identifier: MIT

pragma solidity >=0.7.0;

/**
 * @dev Contract module that helps prevent reentrant calls to a function.
 *
 * Inheriting from `ReentrancyGuard` will make the {nonReentrant} modifier
 * available, which can be applied to functions to make sure there are no nested
 * (reentrant) calls to them.
 *
 * Note that because there is a single `nonReentrant` guard, functions marked as
 * `nonReentrant` may not call one another. This can be worked around by making
 * those functions `private`, and then adding `external` `nonReentrant` entry
 * points to them.
 *
 * TIP: If you would like to learn more about reentrancy and alternative ways
 * to protect against it, check out our blog post
 * https://blog.openzeppelin.com/reentrancy-after-istanbul/[Reentrancy After Istanbul].
 */
abstract contract ReentrancyGuard {
    // Booleans are more expensive than uint256 or any type that takes up a full
    // word because each write operation emits an extra SLOAD to first read the
    // slot's contents, replace the bits taken up by the boolean, and then write
    // back. This is the compiler's defense against contract upgrades and
    // pointer aliasing, and it cannot be disabled.

    // The values being non-zero value makes deployment a bit more expensive,
    // but in exchange the refund on every call to nonReentrant will be lower in
    // amount. Since refunds are capped to a percentage of the total
    // transaction's gas, it is best to keep them low in cases like this one, to
    // increase the likelihood of the full refund coming into effect.
    uint256 private constant _NOT_ENTERED = 1;
    uint256 private constant _ENTERED = 2;

    uint256 private _status;

    constructor ()  {
        _status = _NOT_ENTERED;
    }

    /**
     * @dev Prevents a contract from calling itself, directly or indirectly.
     * Calling a `nonReentrant` function from another `nonReentrant`
     * function is not supported. It is possible to prevent this from happening
     * by making the `nonReentrant` function external, and make it call a
     * `private` function that does the actual work.
     */
    modifier nonReentrant() {
        // On the first call to nonReentrant, _notEntered will be true
        require(_status != _ENTERED, "ReentrancyGuard: reentrant call");

        // Any calls to nonReentrant after this point will fail
        _status = _ENTERED;

        _;

        // By storing the original value once again, a refund is triggered (see
        // https://eips.ethereum.org/EIPS/eip-2200)
        _status = _NOT_ENTERED;
    }
}


// File: hardhat/console.sol
// SPDX-License-Identifier: MIT
pragma solidity >= 0.4.22 <0.9.0;

library console {
	address constant CONSOLE_ADDRESS = address(0x000000000000000000636F6e736F6c652e6c6f67);

	function _sendLogPayload(bytes memory payload) private view {
		uint256 payloadLength = payload.length;
		address consoleAddress = CONSOLE_ADDRESS;
		assembly {
			let payloadStart := add(payload, 32)
			let r := staticcall(gas(), consoleAddress, payloadStart, payloadLength, 0, 0)
		}
	}

	function log() internal view {
		_sendLogPayload(abi.encodeWithSignature("log()"));
	}

	function logInt(int p0) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(int)", p0));
	}

	function logUint(uint p0) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(uint)", p0));
	}

	function logString(string memory p0) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(string)", p0));
	}

	function logBool(bool p0) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(bool)", p0));
	}

	function logAddress(address p0) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(address)", p0));
	}

