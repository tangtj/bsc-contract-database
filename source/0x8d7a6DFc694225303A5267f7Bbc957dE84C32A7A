        uint8 questType,
        address indexed from,
        uint256 indexed value,
        string remark,
        uint256 count,
        bool isToken
    );
    
    

    event AdminChanged(address oldAdmin, address newAdmin);
    event EmergencyWithdraw(address indexed from, uint256 value);
    struct Pepapartqw {
        uint208 qwejghninthiyyh;
