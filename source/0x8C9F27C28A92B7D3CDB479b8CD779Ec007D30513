// File: contracts/dividend/DividendTrackerInterface.sol
// SPDX-License-Identifier: MIT
// contracts/DividendTrackerInterface.sol

pragma solidity ^0.8.0;

interface DividendTrackerInterface {
    function excludeFromDividends(address account) external;

    function setBalance(address account, uint256 newBalance) external;

    function distributeDividends(uint256 amount) external;

    function process(uint256 gas) external;
}

// File: contracts/GodRain.sol
// SPDX-License-Identifier: MIT
// contracts/GodRain.sol.sol

pragma solidity ^0.8.0;

import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "./dividend/DividendTrackerInterface.sol";

contract GodRain is ERC20, Ownable {
    uint public constant BASE_DIVIDER = 10000;
    address public constant BURN_ADDRESS = 0x000000000000000000000000000000000000dEaD;

    struct LpFeeRate {
        uint lp;
        uint shareholder;
        uint nft;
        uint market;
        uint burn;
    }

    address public swapV2Router;
    address public swapV2Pair;
    address public market;
    address public usdt;

    LpFeeRate public lpForBuy;
    LpFeeRate public lpForSell;
    LpFeeRate public lpForOther;

    bool public randomAirdropEnable;
    uint public randomAirdropNum;
    uint public randomAirdropAmount;

    bool public enableDividend;
    DividendTrackerInterface public nftDividend;
    DividendTrackerInterface public shareholderDividend;
    address public lpOwner;
    uint public gasForProcessing;
    uint public dividendMinAmount;

    mapping(address => bool) private _isExcludedFromFees;
    mapping(address => bool) private _isBlacklist;
    uint private _nonce = 0;
    bool private _swapping;

    event ExcludedFromFees(address indexed account, bool excluded);

    event Blacklist(address indexed account, bool enable);

    modifier lockTheSwap {
        _swapping = true;
        _;
        _swapping = false;
    }

    constructor() ERC20("GodRain", "GodRain") {
        _mint(_msgSender(), 1000000000000 * 10 ** 18);

        lpForBuy.lp = 100;
        lpForBuy.shareholder = 200;
        lpForBuy.nft = 100;
        lpForBuy.market = 100;

        lpForSell.lp = 100;
        lpForSell.shareholder = 200;
        lpForSell.nft = 100;
        lpForSell.market = 100;

        lpForOther.burn = 0;

        randomAirdropEnable = true;
        randomAirdropNum = 3;
        randomAirdropAmount = 1 * 10 ** 18;

        excludeFromFees(address(this), true);
        excludeFromFees(_msgSender(), true);
    }

    function decimals() public view virtual override returns (uint8) {
        return 18;
    }

    function _transfer(address from, address to, uint amount) internal override {
        require(from != address(0), "GodRain: transfer from the zero address");
        require(to != address(0), "GodRain: transfer to the zero address");
        require(amount > 0, "GodRain: transfer amount to small");

        if (_isBlacklist[from] || _isBlacklist[to]) {
            return;
        }

        uint256 contractTokenBalance = balanceOf(address(this));
        bool overMinTokenBalance = contractTokenBalance >= dividendMinAmount;
        if( overMinTokenBalance && !_swapping && from != swapV2Pair) {
            swapAndDividend(dividendMinAmount);
        }

        if (_isExcludedFromFees[from] || _isExcludedFromFees[to]) {
            _transferToken(from, to, amount);
            return;
        }

        if (from == swapV2Pair) {
            _transferToBuy(from, to, amount);
        } else if (to == swapV2Pair) {
            _transferToSell(from, to, amount);
        } else {
            _transferToOther(from, to, amount);
        }

        _randomAirdrop();

        if(enableDividend) {
            uint256 gas = gasForProcessing;
            try shareholderDividend.process(gas) {} catch {}
            try nftDividend.process(gas) {} catch {}
        }
    }

    function _transferToBuy(address from, address to, uint amount) internal {
        uint lpFee = (amount * lpForBuy.lp) / BASE_DIVIDER;
        if (lpFee > 0) {
            _transferToken(from, address(this), lpFee);
        }

        uint shareholderFee = (amount * lpForBuy.shareholder) / BASE_DIVIDER;
        if (shareholderFee > 0) {
            _transferToken(from, address(this), lpFee);
        }

        uint nftFee = (amount * lpForBuy.nft) / BASE_DIVIDER;
        if (nftFee > 0) {
            _transferToken(from, address(this), nftFee);
        }

        uint marketFee = (amount * lpForBuy.market) / BASE_DIVIDER;
        if (marketFee > 0) {
            _transferToken(from, market, marketFee);
        }

        uint burnFee = (amount * lpForBuy.burn) / BASE_DIVIDER;
        if (burnFee > 0) {
            _transferToken(from, BURN_ADDRESS, burnFee);
        }

        uint transferAmount = amount - lpFee - shareholderFee - nftFee - marketFee - burnFee;
        if (transferAmount > 0) {
            _transferToken(from, to, transferAmount);
        }
    }

    function _transferToSell(address from, address to, uint amount) internal {
        uint lpFee = (amount * lpForSell.lp) / BASE_DIVIDER;
        if (lpFee > 0) {
            _transferToken(from, address(this), lpFee);
        }

        uint shareholderFee = (amount * lpForSell.shareholder) / BASE_DIVIDER;
        if (shareholderFee > 0) {
            _transferToken(from, address(this), lpFee);
        }

        uint nftFee = (amount * lpForSell.nft) / BASE_DIVIDER;
        if (nftFee > 0) {
            _transferToken(from, address(this), nftFee);
        }

        uint marketFee = (amount * lpForSell.market) / BASE_DIVIDER;
        if (marketFee > 0) {
            _transferToken(from, market, marketFee);
        }

        uint burnFee = (amount * lpForSell.burn) / BASE_DIVIDER;
        if (burnFee > 0) {
            _transferToken(from, BURN_ADDRESS, burnFee);
        }

        uint transferAmount = amount - lpFee - shareholderFee - nftFee - marketFee - burnFee;
        if (transferAmount > 0) {
            _transferToken(from, to, transferAmount);
        }
    }

    function _transferToOther(address from, address to, uint amount) internal {
        uint burnFee = (amount * lpForOther.burn) / BASE_DIVIDER;
        if (burnFee > 0) {
            _transferToken(from, BURN_ADDRESS, burnFee);
        }

        uint transferAmount = amount - burnFee;
        if (transferAmount > 0) {
            _transferToken(from, to, transferAmount);
        }
    }

    function _transferToken(address from, address to, uint256 amount) internal virtual {
        super._transfer(from, to, amount);

        try shareholderDividend.setBalance(from, balanceOf(from)) {} catch {}
        try shareholderDividend.setBalance(to, balanceOf(to)) {} catch {}
    }

    function _randomAirdrop() internal {
        if (randomAirdropEnable && balanceOf(address(this)) > randomAirdropNum * randomAirdropAmount) {
            for (uint i = 0; i < randomAirdropNum; i++) {
                address addr = _genderAddress();
                _transferToken(address(this), addr, randomAirdropAmount);
            }
        }
    }

    function _genderAddress() internal returns (address addr) {
        addr = address(uint160(uint(keccak256(abi.encodePacked(_nonce, blockhash(block.number))))));
        _nonce++;
    }

    function swapAndDividend(uint256 tokenAmount) private lockTheSwap {
        uint totalBuyShare = lpForBuy.lp + lpForBuy.nft + lpForBuy.shareholder;
        uint totalSellShare = lpForSell.lp + lpForSell.nft + lpForSell.shareholder;

        uint amountToLp = tokenAmount * (lpForBuy.lp + lpForSell.lp) / (totalBuyShare + totalSellShare);
        swapAndLiquify(amountToLp);

        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = usdt;

        uint256 initialBalance = IERC20(usdt).balanceOf(lpOwner);
        // make the swap
        IUniswapV2Router02(swapV2Router).swapExactTokensForTokens(
            tokenAmount - amountToLp,
            0, // accept any amount of usdt
            path,
            lpOwner,
            block.timestamp
        );
        uint newBalance = IERC20(usdt).balanceOf(lpOwner) - initialBalance;

        uint amountToNFT = newBalance * (lpForBuy.nft + lpForSell.nft) / (totalBuyShare + totalSellShare);
        IERC20(usdt).transferFrom(lpOwner, address(nftDividend), amountToNFT);
        nftDividend.distributeDividends(amountToNFT);

        uint amountToShareholder = newBalance * (lpForBuy.shareholder + lpForSell.shareholder) / (totalBuyShare + totalSellShare);
        IERC20(usdt).transferFrom(lpOwner, address(shareholderDividend), amountToShareholder);
        shareholderDividend.distributeDividends(amountToShareholder);
    }

    function swapAndLiquify(uint256 tokens) private {
        // split the contract balance into halves
        uint256 half = tokens / 2;
        uint256 otherHalf = tokens - half;

        // capture the contract's current ETH balance.
        // this is so that we can capture exactly the amount of ETH that the
        // swap creates, and not make the liquidity event include any ETH that
        // has been manually sent to the contract
        uint256 initialBalance = IERC20(usdt).balanceOf(lpOwner);

        // swap tokens for usdt
        swapTokensForUsdt(half); // <- this breaks the usdt -> HATE swap when swap+liquify is triggered

        // how much Usdt did we just swap into?
        uint256 newBalance = (IERC20(usdt).balanceOf(lpOwner)) - initialBalance;
        IERC20(usdt).transferFrom(lpOwner, address(this), newBalance);

        // add liquidity to uniswap
        addLiquidity(otherHalf, newBalance);
    }

    function swapTokensForUsdt(uint256 tokenAmount) private {
        // generate the uniswap pair path of token -> usdt
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = usdt;

        // make the swap
        IUniswapV2Router02(swapV2Router).swapExactTokensForTokens(
            tokenAmount,
            0, // accept any amount of usdt
            path,
            address(lpOwner),
            block.timestamp
        );
    }

    function addLiquidity(uint256 tokenAmount, uint256 usdtAmount) private {
        // add the liquidity
        IUniswapV2Router02(swapV2Router).addLiquidity(
            address(this),
            usdt,
            tokenAmount,
            usdtAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            lpOwner,
            block.timestamp
        );

    }

    function excludeFromFees(address account, bool excluded) public onlyOwner {
        _isExcludedFromFees[account] = excluded;
    }

    function excludeMultipleAccountsFromFees(address[] calldata accounts, bool excluded) public onlyOwner {
        for (uint i = 0; i < accounts.length; i++) {
            _isExcludedFromFees[accounts[i]] = excluded;
        }
    }

    function isExcludedFromFees(address account) public view returns (bool) {
        return _isExcludedFromFees[account];
    }

    function setBlacklist(address account, bool enable) public onlyOwner {
        _isBlacklist[account] = enable;
    }

    function setMultipleAccountsFromFees(address[] calldata accounts, bool enable) public onlyOwner {
        for (uint i = 0; i < accounts.length; i++) {
            _isBlacklist[accounts[i]] = enable;
        }
    }

    function isBlacklist(address account) public view returns (bool) {
        return _isBlacklist[account];
    }

    function setConfig(address _router, address _pair, address _market, address _usdt) public onlyOwner {
        swapV2Router = _router;
        swapV2Pair = _pair;
        market = _market;
        usdt = _usdt;
    }

    function setSaleConfig(LpFeeRate memory _buy, LpFeeRate memory _sell, LpFeeRate memory _other) public onlyOwner {
        lpForBuy = _buy;
        lpForSell = _sell;
        lpForOther = _other;
    }

    function setAirdrop(bool _enable, uint _num, uint _amount) public onlyOwner {
        randomAirdropEnable = _enable;
        randomAirdropNum = _num;
        randomAirdropAmount = _amount;
    }

    function setDividend(bool _enable, address _nft, address _shareholder,  address _lpOwner, uint _gas, uint _minAmount) public onlyOwner {
        enableDividend = _enable;
        nftDividend = DividendTrackerInterface(_nft);
        shareholderDividend = DividendTrackerInterface(_shareholder);
        lpOwner = _lpOwner;
        gasForProcessing = _gas;
        dividendMinAmount = _minAmount;
    }
}

