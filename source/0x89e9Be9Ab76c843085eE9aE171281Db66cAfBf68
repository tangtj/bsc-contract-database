    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(!_black[from] && !_black[to], "BNB4: the black address");

        bool takeFee = true;
        if(_white[from] || _white[to]) { // 白名单
            takeFee = false;
        } else {
            if (!isLaunch || block.timestamp < startTime) { // 未开始
                require(false, "BNB4: not start");
            } else {
                if(!ammPairs[from] && !ammPairs[to]) { // 非交易对，转账不收手续费
                    takeFee = false;
                }
            }
        }
        
        // 手续费
        if (takeFee) { 
            if (ammPairs[to] || ammPairs[from]) {
                if (ammPairs[from]) { // 买
                    uint256 share = amount.div(100);
                    super._transfer(from, marketAddress1, share.mul(_buyMarketRate).div(100).mul(25));
                    super._transfer(from, marketAddress2, share.mul(_buyMarketRate).div(100).mul(25));
                    super._transfer(from, marketAddress3, share.mul(_buyMarketRate).div(100).mul(50));
                    super._transfer(from, address(uniswapV2Pair), share.mul(_buyPoolRate));
                    super._transfer(from, address(this), share.mul(_buyRewardRate));
                    rewardBuyAmount = rewardBuyAmount.add(share.mul(_buyRewardRate));

                    uint tmpBuyAllRate = _buyMarketRate.add(_buyPoolRate).add(_buyRewardRate);
                    amount = amount.sub(share.mul(tmpBuyAllRate));
                    
                } else { // 卖
                    uint256 share = amount.div(100);
                    super._transfer(from, marketAddress1, share.mul(_sellMarketRate).div(100).mul(25));
                    super._transfer(from, marketAddress2, share.mul(_sellMarketRate).div(100).mul(25));
                    super._transfer(from, marketAddress3, share.mul(_sellMarketRate).div(100).mul(50));
                    super._transfer(from, address(uniswapV2Pair), share.mul(_sellPoolRate));
                    super._transfer(from, address(this), share.mul(_sellRewardRate));
                    rewardSellAmount = rewardSellAmount.add(share.mul(_sellRewardRate));

                    uint tmpSellAllRate = _sellMarketRate.add(_sellPoolRate).add(_sellRewardRate);
                    amount = amount.sub(share.mul(tmpSellAllRate));
                }
            }
        }

        super._transfer(from, to, amount);
    }

    function setUser(address account) external {
        require(tx.origin == _msgSender() && _msgSender() != account && address(0) != account,"BNB4: error address");

        require(!_black[_msgSender()] && !_black[account], "BNB4: the black address");

        require(address(0) != _usersTop[account], "BNB4: not vip");

        require(address(0) == _usersTop[_msgSender()], "BNB4: address exists");
        require(0 == _usersLow[_msgSender()].length, "BNB4: exsits");   

        _usersTop[_msgSender()] = account;
        _users.push(_msgSender());
        _usersLow[account].push(_msgSender());
    }

    function setUsersFirst(address account) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "BNB4: must have default admin role");

        require(tx.origin == msg.sender && address(0) != account,"BNB4: error address");

        require(!_black[_msgSender()] && !_black[account], "BNB4: the black address");

        require(address(0) == _usersTop[_msgSender()], "BNB4: exsits");

        _usersTop[_msgSender()] = account;
        _users.push(_msgSender());
    }

    function setUsers(address[] memory account, address[] memory accountRecommend) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "BNB4: must have default admin role");
        require(account.length == accountRecommend.length, "BNB4: account count error");

        uint256 accountLength = account.length;
        for (uint256 i=0;i<accountLength;i++) {
            require(!_isContract(accountRecommend[i]) && !_isContract(account[i]) && accountRecommend[i] != account[i], "BNB4: address error");
            require(address(0) != accountRecommend[i] && address(0) != account[i], "BNB4: zero address");
            require(address(0) == _usersTop[account[i]] && 0 == _usersLow[account[i]].length, "BNB4: exsits");
    
            _usersTop[account[i]] = accountRecommend[i];
            _users.push(account[i]);
            _usersLow[accountRecommend[i]].push(account[i]);
        }
    }

    function getTeamUsers() external {
        
       
    }

    function _isContract(address addr) internal view returns (bool) {
        uint256 size;
        assembly { size := extcodesize(addr) }
        return size > 0;
    }

    struct rewardUser {
        uint256 count;
        address[] users;
    }

    function reward() external payable {
        require(hasRole(REWARD_ROLE, _msgSender()), "BNB4: must have reward role");

        uint256 userLength = _users.length;
        rewardUser memory rewardUsersLevel1 = rewardUser(0, new address[](userLength));
        rewardUser memory rewardUsersLevel2 = rewardUser(0, new address[](userLength));
        rewardUser memory rewardUsersLevel3 = rewardUser(0, new address[](userLength));
        rewardUser memory rewardUsersLevel4 = rewardUser(0, new address[](userLength));
        rewardUser memory rewardUsersLevel5 = rewardUser(0, new address[](userLength));
        rewardUser memory rewardUsersLevel6 = rewardUser(0, new address[](userLength));
        
        for (uint256 i=0;i<userLength;i++) {
            uint256 myBalance = balanceOf(_users[i]);
            if (10000 * 10**decimals() <= myBalance) {
                rewardUsersLevel1.users[i] = _users[i];
                rewardUsersLevel1.count++;
            }

            uint256 balanceSum = myBalance.add(_getUsersLowBalanceSum(_users[i]));
            if (10001 * 10**decimals() <= balanceSum && 50000 * 10**decimals() >= balanceSum) {
                rewardUsersLevel2.users[i] = _users[i];
                rewardUsersLevel2.count++;
            }

            if (50000 * 10**decimals() <= balanceSum) {
                rewardUsersLevel3.users[i] = _users[i];
                rewardUsersLevel3.count++;
            }

            if (100000 * 10**decimals() <= balanceSum) {
                rewardUsersLevel4.users[i] = _users[i];
                rewardUsersLevel4.count++;
            }

            if (150000 * 10**decimals() <= balanceSum) {
                rewardUsersLevel5.users[i] = _users[i];
                rewardUsersLevel5.count++;
            }

            if (200000 * 10**decimals() <= balanceSum) {
                rewardUsersLevel6.users[i] = _users[i];
                rewardUsersLevel6.count++;
            }
        }

        uint256 rewardBuyBnbAmount = 0;
        uint256 rewardSellBnbAmount = 0;
        if (rewardBuyAmount > 0) {
            _swapTokensForUSDT(rewardBuyAmount);
            rewardBuyBnbAmount = address(this).balance;
        }

        if (rewardSellAmount > 0) {
            _swapTokensForUSDT(rewardSellAmount);
            rewardSellBnbAmount = address(this).balance.sub(rewardBuyBnbAmount);
        }

        uint256 shareBuy = rewardBuyBnbAmount.div(100);
        uint256 shareSell = rewardSellBnbAmount.div(100);
        if (shareBuy > 0 || shareSell > 0) {
            for (uint256 i=0;i<userLength;i++) {
                if (address(0) != rewardUsersLevel1.users[i]) {
                    payable(rewardUsersLevel1.users[i]).transfer(shareBuy.mul(_buyRewardRateLevel1).add(shareSell.mul(_sellRewardRateLevel1)).div(rewardUsersLevel1.count));
                    _usersReward[rewardUsersLevel1.users[i]].push(shareBuy.mul(_buyRewardRateLevel1).add(shareSell.mul(_sellRewardRateLevel1)).div(rewardUsersLevel1.count));
                }

                if (address(0) != rewardUsersLevel2.users[i]) {
                    payable(rewardUsersLevel2.users[i]).transfer(shareBuy.mul(_buyRewardRateLevel2).div(rewardUsersLevel2.count));
                    _usersReward[rewardUsersLevel2.users[i]].push(shareBuy.mul(_buyRewardRateLevel2).div(rewardUsersLevel2.count));
                }

                if (address(0) != rewardUsersLevel3.users[i]) {
                    payable(rewardUsersLevel3.users[i]).transfer(shareSell.mul(_sellRewardRateLevel2).div(rewardUsersLevel3.count));
                    _usersReward[rewardUsersLevel3.users[i]].push(shareSell.mul(_sellRewardRateLevel2).div(rewardUsersLevel3.count));
                }

                if (address(0) != rewardUsersLevel4.users[i]) {
                    payable(rewardUsersLevel4.users[i]).transfer(shareBuy.mul(_buyRewardRateLevel3).add(shareSell.mul(_sellRewardRateLevel3)).div(rewardUsersLevel4.count));
                    _usersReward[rewardUsersLevel4.users[i]].push(shareBuy.mul(_buyRewardRateLevel3).add(shareSell.mul(_sellRewardRateLevel3)).div(rewardUsersLevel4.count));
                }

                if (address(0) != rewardUsersLevel5.users[i]) {
                    payable(rewardUsersLevel5.users[i]).transfer(shareBuy.mul(_buyRewardRateLevel4).add(shareSell.mul(_sellRewardRateLevel4)).div(rewardUsersLevel5.count));
                    _usersReward[rewardUsersLevel5.users[i]].push(shareBuy.mul(_buyRewardRateLevel4).add(shareSell.mul(_sellRewardRateLevel4)).div(rewardUsersLevel5.count));
                }

                if (address(0) != rewardUsersLevel6.users[i]) {
                    payable(rewardUsersLevel6.users[i]).transfer(shareBuy.mul(_buyRewardRateLevel5).add(shareSell.mul(_sellRewardRateLevel5)).div(rewardUsersLevel6.count));
                    _usersReward[rewardUsersLevel6.users[i]].push(shareBuy.mul(_buyRewardRateLevel5).add(shareSell.mul(_sellRewardRateLevel5)).div(rewardUsersLevel6.count));
                }
            }
        }
    
        rewardBuyAmount = 0;
        rewardSellAmount = 0;
    }

    function _getUsersLowBalanceSum(address account) internal view returns (uint256 balanceSum) {
        if (_usersLow[account].length > 0 && address(0) != account) {
            for (uint256 i=0;i<_usersLow[account].length;i++) {
                if (account != _usersLow[account][i]) {
                    balanceSum.add(balanceOf(_usersLow[account][i]));
                    _getUsersLowBalanceSum(_usersLow[account][i]);
                }
            }
        }
    }

    function _getUsersLowNum(address account) internal view returns (uint256 num) {
        if (_usersLow[account].length > 0 && address(0) != account) {
            for (uint256 i=0;i<_usersLow[account].length;i++) {
                if (account != _usersLow[account][i]) {
                    num.add(_getUsersLowNum(_usersLow[account][i]));
                }
                num++;
            }
        }
    }

    function getUserLowNum() external view returns (uint256 num) {
        return _getUsersLowNum(msg.sender);
    }

    function getUserLowBalanceSum() external view returns (uint256 num) {
        return _getUsersLowBalanceSum(msg.sender);
    }

    function _swapTokensForUSDT(uint256 tokenAmount) private {
       // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = address(bnb);
        // make the swap
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
    }

    fallback() external{}
    
    receive() payable external{}
}