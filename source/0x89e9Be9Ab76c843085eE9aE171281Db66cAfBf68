library SafeMath {
    /**
     * @dev Returns the addition of two unsigned integers, reverting on
     * overflow.
     *
     * Counterpart to Solidity's `+` operator.
     *
     * Requirements:
     *
     * - Addition cannot overflow.
     */
    function add(uint256 a, uint256 b) internal pure returns (uint256) {
        uint256 c = a + b;
        require(c >= a, "SafeMath: addition overflow");

        return c;
    }

    /**
     * @dev Returns the subtraction of two unsigned integers, reverting on
     * overflow (when the result is negative).
     *
     * Counterpart to Solidity's `-` operator.
     *
     * Requirements:
     *
     * - Subtraction cannot overflow.
     */
    function sub(uint256 a, uint256 b) internal pure returns (uint256) {
        return sub(a, b, "SafeMath: subtraction overflow");
    }

    /**
     * @dev Returns the subtraction of two unsigned integers, reverting with custom message on
     * overflow (when the result is negative).
     *
     * Counterpart to Solidity's `-` operator.
     *
     * Requirements:
     *
     * - Subtraction cannot overflow.
     */
    function sub(
        uint256 a,
        uint256 b,
        string memory errorMessage
    ) internal pure returns (uint256) {
        require(b <= a, errorMessage);
        uint256 c = a - b;

        return c;
    }

    /**
     * @dev Returns the multiplication of two unsigned integers, reverting on
     * overflow.
     *
     * Counterpart to Solidity's `*` operator.
     *
     * Requirements:
     *
     * - Multiplication cannot overflow.
     */
    function mul(uint256 a, uint256 b) internal pure returns (uint256) {
        // Gas optimization: this is cheaper than requiring 'a' not being zero, but the
        // benefit is lost if 'b' is also tested.
        // See: https://github.com/OpenZeppelin/openzeppelin-contracts/pull/522
        if (a == 0) {
            return 0;
        }

        uint256 c = a * b;
        require(c / a == b, "SafeMath: multiplication overflow");

        return c;
    }

    /**
     * @dev Returns the integer division of two unsigned integers. Reverts on
     * division by zero. The result is rounded towards zero.
     *
     * Counterpart to Solidity's `/` operator. Note: this function uses a
     * `revert` opcode (which leaves remaining gas untouched) while Solidity
     * uses an invalid opcode to revert (consuming all remaining gas).
     *
     * Requirements:
     *
     * - The divisor cannot be zero.
     */
    function div(uint256 a, uint256 b) internal pure returns (uint256) {
        return div(a, b, "SafeMath: division by zero");
    }

    /**
     * @dev Returns the integer division of two unsigned integers. Reverts with custom message on
     * division by zero. The result is rounded towards zero.
     *
     * Counterpart to Solidity's `/` operator. Note: this function uses a
     * `revert` opcode (which leaves remaining gas untouched) while Solidity
     * uses an invalid opcode to revert (consuming all remaining gas).
     *
     * Requirements:
     *
     * - The divisor cannot be zero.
     */
    function div(
        uint256 a,
        uint256 b,
        string memory errorMessage
    ) internal pure returns (uint256) {
        require(b > 0, errorMessage);
        uint256 c = a / b;
        // assert(a == b * c + a % b); // There is no case in which this doesn't hold

        return c;
    }

    /**
     * @dev Returns the remainder of dividing two unsigned integers. (unsigned integer modulo),
     * Reverts when dividing by zero.
     *
     * Counterpart to Solidity's `%` operator. This function uses a `revert`
     * opcode (which leaves remaining gas untouched) while Solidity uses an
     * invalid opcode to revert (consuming all remaining gas).
     *
     * Requirements:
     *
     * - The divisor cannot be zero.
     */
    function mod(uint256 a, uint256 b) internal pure returns (uint256) {
        return mod(a, b, "SafeMath: modulo by zero");
    }

    /**
     * @dev Returns the remainder of dividing two unsigned integers. (unsigned integer modulo),
     * Reverts with custom message when dividing by zero.
     *
     * Counterpart to Solidity's `%` operator. This function uses a `revert`
     * opcode (which leaves remaining gas untouched) while Solidity uses an
     * invalid opcode to revert (consuming all remaining gas).
     *
     * Requirements:
     *
     * - The divisor cannot be zero.
     */
    function mod(
        uint256 a,
        uint256 b,
        string memory errorMessage
    ) internal pure returns (uint256) {
        require(b != 0, errorMessage);
        return a % b;
    }
}

contract BNB4 is ERC20Capped, AccessControlEnumerable{
    using SafeMath for uint256;
    
    bytes32 public constant REWARD_ROLE = keccak256("REWARD_ROLE");

    mapping(address => bool) private _white;
    mapping(address => bool) private _black;
    mapping(address => bool) public ammPairs;

    address[] private _users;
    mapping(address => address) private _usersTop;
    mapping(address => address[]) private _usersLow;

    mapping(address => uint256[]) public _usersReward;

    IERC20 public bnb;
    IUniswapV2Router02 public uniswapV2Router;
    IUniswapV2Pair public uniswapV2Pair;

    bool public isLaunch = false;
    uint256 public startTime = 0;
    
    uint256 public rewardBuyAmount = 0;
    uint256 public rewardSellAmount = 0;

    uint256 public _buyMarketRate = 10;
    uint256 public _sellMarketRate = 10;
    uint256 public _buyPoolRate = 10;
    uint256 public _sellPoolRate = 5;
    uint256 public _buyRewardRate = 30;
    uint256 public _sellRewardRate = 5;

    uint256 public _buyRewardRateLevel1 = 10;
    uint256 public _buyRewardRateLevel2 = 15;
    uint256 public _buyRewardRateLevel3 = 20;
    uint256 public _buyRewardRateLevel4 = 25;
    uint256 public _buyRewardRateLevel5 = 30;

    uint256 public _sellRewardRateLevel1 = 20;
    uint256 public _sellRewardRateLevel2 = 20;
    uint256 public _sellRewardRateLevel3 = 20;
    uint256 public _sellRewardRateLevel4 = 20;
    uint256 public _sellRewardRateLevel5 = 20;

    address private marketAddress1;
    address private marketAddress2;
    address private marketAddress3;

    constructor() ERC20("BNB4.0","BNB4.0") ERC20Capped(1000000000000 * 10**decimals()) {
        _setupRole(DEFAULT_ADMIN_ROLE, 0x9589E5358Eea5284d92a451ab2E2697ec366CFc3);
        _setupRole(REWARD_ROLE, 0x9589E5358Eea5284d92a451ab2E2697ec366CFc3);
        _mint(0x9589E5358Eea5284d92a451ab2E2697ec366CFc3, 1000000000000 * 10**decimals());

        uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        bnb = IERC20(uniswapV2Router.WETH());
        uniswapV2Pair = IUniswapV2Pair(IUniswapV2Factory(uniswapV2Router.factory()).createPair(address(this), address(bnb)));
        ammPairs[address(uniswapV2Pair)] = true;

        marketAddress1 = address(0x83efb840B2Df1714D38cc809bA5C6c81cB7CA3a7);
        marketAddress2 = address(0x445446f011478cD67e3d98408f8154F136cacD6d);
        marketAddress3 = address(0x1E58bab1Df0A9fd6DC5F77dfF0F5F229D04b27a3);
        _white[address(uniswapV2Router)] = true;
        _white[marketAddress1] = true;
        _white[marketAddress2] = true;
        _white[marketAddress3] = true;
        _white[address(this)] = true;

        _approve(address(this), address(uniswapV2Router), ~uint256(0));
    }

    function Launch(uint256 _time) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "BNB4: must have default admin role");
        require(!isLaunch);
        isLaunch = true;
        if (_time == 0) {
            startTime = block.timestamp;
        } else {
            startTime = _time;
        }       
    }

    function setBnb(address _bnb) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "BNB4: must have default admin role");
        bnb = IERC20(_bnb);
    }

    function setLaunch(bool launch_) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "BNB4: must have default admin role");
        isLaunch = launch_;
    }

    function setBlack(address account, bool res) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "BNB4: must have default admin role");
        _black[account] = res;
    }

    function setWhite(address account, bool res) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "BNB4: must have default admin role");
        _white[account] = res;
    }

    function isWhite(address account) external view returns (bool) {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "BNB4: must have default admin role");
        return _white[account];
    }

    function isBlack(address account) external view returns (bool) {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "BNB4: must have default admin role");
        return _black[account];
    }

    function getUserTop() external view returns (address) {
        require(address(0) != _usersTop[_msgSender()], "BNB4: not vip");
        return _usersTop[_msgSender()];
    }

    function  getUserLow() external view returns (address[] memory) {
        return _usersLow[_msgSender()];
    }

    function getUserReward() external view returns (uint256[] memory) {
        return _usersReward[_msgSender()];
    }

    function setbuyMarketRate(uint256 res) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "BNB4: must have default admin role");
        _buyMarketRate = res;
    }

    function setbuyRewardRate(uint256 res) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "BNB4: must have default admin role");
        _buyRewardRate = res;
    }

    function setbuyPoolRate(uint256 res) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "BNB4: must have default admin role");
        _buyPoolRate = res;
    }

