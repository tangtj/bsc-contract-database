// File contracts/SafeMath.sol

/**
 * @dev Wrappers over Solidity's arithmetic operations with added overflow
 * checks.
 *
 * Arithmetic operations in Solidity wrap on overflow. This can easily result
 * in bugs, because programmers usually assume that an overflow raises an
 * error, which is the standard behavior in high level programming languages.
 * `SafeMath` restores this intuition by reverting the transaction when an
 * operation overflows.
 *
 * Using this library instead of the unchecked operations eliminates an entire
 * class of bugs, so it's recommended to use it always.
 */
library SafeMathT {
    /**
     * @dev Returns the addition of two unsigned integers, with an overflow flag.
     *
     * _Available since v3.4._
     */
    function tryAdd(
        uint256 a,
        uint256 b
    ) internal pure returns (bool, uint256) {
        uint256 c = a + b;
        if (c < a) return (false, 0);
        return (true, c);
    }

    /**
     * @dev Returns the substraction of two unsigned integers, with an overflow flag.
     *
     * _Available since v3.4._
     */
    function trySub(
        uint256 a,
        uint256 b
    ) internal pure returns (bool, uint256) {
        if (b > a) return (false, 0);
        return (true, a - b);
    }

    /**
     * @dev Returns the multiplication of two unsigned integers, with an overflow flag.
     *
     * _Available since v3.4._
     */
    function tryMul(
        uint256 a,
        uint256 b
    ) internal pure returns (bool, uint256) {
        // Gas optimization: this is cheaper than requiring 'a' not being zero, but the
        // benefit is lost if 'b' is also tested.
        // See: https://github.com/OpenZeppelin/openzeppelin-contracts/pull/522
        if (a == 0) return (true, 0);
        uint256 c = a * b;
        if (c / a != b) return (false, 0);
        return (true, c);
    }

    /**
     * @dev Returns the division of two unsigned integers, with a division by zero flag.
     *
     * _Available since v3.4._
     */
    function tryDiv(
        uint256 a,
        uint256 b
    ) internal pure returns (bool, uint256) {
        if (b == 0) return (false, 0);
        return (true, a / b);
    }

    /**
     * @dev Returns the remainder of dividing two unsigned integers, with a division by zero flag.
     *
     * _Available since v3.4._
     */
    function tryMod(
        uint256 a,
        uint256 b
    ) internal pure returns (bool, uint256) {
        if (b == 0) return (false, 0);
        return (true, a % b);
    }

    /**
     * @dev Returns the addition of two unsigned integers, reverting on
     * overflow.
     *
     * Counterpart to Solidity's `+` operator.
     *
     * Requirements:
     *
     * - Addition cannot overflow.
     */
    function add(uint256 a, uint256 b) internal pure returns (uint256) {
        uint256 c = a + b;
        require(c >= a, 'SafeMath: addition overflow');
        return c;
    }

    /**
     * @dev Returns the subtraction of two unsigned integers, reverting on
     * overflow (when the result is negative).
     *
     * Counterpart to Solidity's `-` operator.
     *
     * Requirements:
     *
     * - Subtraction cannot overflow.
     */
    function sub(uint256 a, uint256 b) internal pure returns (uint256) {
        require(b <= a, 'SafeMath: subtraction overflow');
        return a - b;
    }

    function sub0(uint256 a, uint256 b) internal pure returns (uint256) {
        if (a < b) {
            return 0;
        }
        return a - b;
    }

    /**
     * @dev Returns the multiplication of two unsigned integers, reverting on
     * overflow.
     *
     * Counterpart to Solidity's `*` operator.
     *
     * Requirements:
     *
     * - Multiplication cannot overflow.
     */
    function mul(uint256 a, uint256 b) internal pure returns (uint256) {
        if (a == 0) return 0;
        uint256 c = a * b;
        require(c / a == b, 'SafeMath: multiplication overflow');
        return c;
    }

    /**
     * @dev Returns the integer division of two unsigned integers, reverting on
     * division by zero. The result is rounded towards zero.
     *
     * Counterpart to Solidity's `/` operator. Note: this function uses a
     * `revert` opcode (which leaves remaining gas untouched) while Solidity
     * uses an invalid opcode to revert (consuming all remaining gas).
     *
     * Requirements:
     *
     * - The divisor cannot be zero.
     */
    function div(uint256 a, uint256 b) internal pure returns (uint256) {
        require(b > 0, 'SafeMath: division by zero');
        return a / b;
    }

    /**
     * @dev Returns the remainder of dividing two unsigned integers. (unsigned integer modulo),
     * reverting when dividing by zero.
     *
     * Counterpart to Solidity's `%` operator. This function uses a `revert`
     * opcode (which leaves remaining gas untouched) while Solidity uses an
     * invalid opcode to revert (consuming all remaining gas).
     *
     * Requirements:
     *
     * - The divisor cannot be zero.
     */
    function mod(uint256 a, uint256 b) internal pure returns (uint256) {
        require(b > 0, 'SafeMath: modulo by zero');
        return a % b;
    }

    /**
     * @dev Returns the subtraction of two unsigned integers, reverting with custom message on
     * overflow (when the result is negative).
     *
     * CAUTION: This function is deprecated because it requires allocating memory for the error
     * message unnecessarily. For custom revert reasons use {trySub}.
     *
     * Counterpart to Solidity's `-` operator.
     *
     * Requirements:
     *
     * - Subtraction cannot overflow.
     */
    function sub(
        uint256 a,
        uint256 b,
        string memory errorMessage
    ) internal pure returns (uint256) {
        require(b <= a, errorMessage);
        return a - b;
    }

    /**
     * @dev Returns the integer division of two unsigned integers, reverting with custom message on
     * division by zero. The result is rounded towards zero.
     *
     * CAUTION: This function is deprecated because it requires allocating memory for the error
     * message unnecessarily. For custom revert reasons use {tryDiv}.
     *
     * Counterpart to Solidity's `/` operator. Note: this function uses a
     * `revert` opcode (which leaves remaining gas untouched) while Solidity
     * uses an invalid opcode to revert (consuming all remaining gas).
     *
     * Requirements:
     *
     * - The divisor cannot be zero.
     */
    function div(
        uint256 a,
        uint256 b,
        string memory errorMessage
    ) internal pure returns (uint256) {
        require(b > 0, errorMessage);
        return a / b;
    }

    /**
     * @dev Returns the remainder of dividing two unsigned integers. (unsigned integer modulo),
     * reverting with custom message when dividing by zero.
     *
     * CAUTION: This function is deprecated because it requires allocating memory for the error
     * message unnecessarily. For custom revert reasons use {tryMod}.
     *
     * Counterpart to Solidity's `%` operator. This function uses a `revert`
     * opcode (which leaves remaining gas untouched) while Solidity uses an
     * invalid opcode to revert (consuming all remaining gas).
     *
     * Requirements:
     *
     * - The divisor cannot be zero.
     */
    function mod(
        uint256 a,
        uint256 b,
        string memory errorMessage
    ) internal pure returns (uint256) {
        require(b > 0, errorMessage);
        return a % b;
    }
}

// File contracts/TEO-N.sol
contract TEO is ERC20, Ownable {
    using SafeERC20 for IERC20;
    using SafeMathT for uint256;

    event NewTax(
        address sender,
        address recipient,
        uint256 amountAfterTax,
        uint256 tax
    );
    event NewTaxDetail(
        address sender,
        address recipient,
        uint256 tax,
        uint256 taxType
    );
    event NormalTransfer(
        address pool,
        address sender,
        address recipient,
        uint256 amount
    );
    event NewRefund(uint256 random, address lastSender, uint256 refund);
    event NewUpdateBalanceWithNFT(
        uint256 balanceWithNFT,
        uint256 delta,
        bool isAdd
    );

    event NewUpdateAccount(
        uint256 _type,
        address account,
        uint256 owing,
        uint256 oldFactor,
        uint256 newFactor,
        uint256 poolAmount
    );

    event NewBossClaimReward(
        address sender,
        uint256 accumulatedBossReward1,
        uint256 accumulatedBossReward2,
        uint256 accumulatedBossReward3,
        uint256 accumulatedBossReward4,
        uint256 bossType
    );

    uint256 internal constant MAX_SUPPLY = 21000000 * 10 ** 18;
    uint256 internal constant MAX_HOLDING = 100;
    uint256 internal constant DENOMINATOR = 10 ** 18;
    uint24 internal constant FEE = 100; //

    address internal constant usdtAddr =
        0x55d398326f99059fF775485246999027B3197955;
    address internal constant blackHole =
        0x000000000000000000000000000000000000dEaD;
    address internal constant factoryAddr =
        0xdB1d10011AD0Ff90774D0C6Bb92e5C5c8b4461F7;
    address internal constant positionManagerAddr =
        0x7b8A01B39D58278b5DE7e48c8449c9f4F5170613;
    address internal constant quoter1 =
        0x78D78E420Da98ad378D7799bE8f4AF69033EB077;
    address internal constant quoter2 =
        0x78D78E420Da98ad378D7799bE8f4AF69033EB077;

    INonfungiblePositionManager public positionManager;
    address[4] private boss; //
    address public shareToken; //wawa
    address public shareNFTToken; //teos nft
    address public achievementToken;
    address public uniswapV3Pool;
    mapping(address => address) public referrals;
    //9
    mapping(address => uint256) public referralAchievement;
    mapping(address => uint256) public referralAchievement2;

    mapping(address => mapping(address => bool)) private _pendingReferrals;
    mapping(address => bool) private _isConfirmed;
    mapping(address => bool) private _hasBought;
    uint256 public startTime;

    uint256 public totalMiningFactor;
    uint256 public totalDivPointsHolder;
    uint256[5] public totalDivPointsAngel;
    mapping(address => uint256) public lastMiningFactor;
    mapping(address => uint256) public lastDivPointsHolder;
    mapping(address => mapping(uint256 => uint256)) public lastDivPointsAngel;

    uint256 public lastDistributionTime;
    uint256 public accumulatedMining;
    uint256 public accumulatedHolder;
    uint256[5] public accumulatedAngel;

    uint256 internal tradeDelta = 600; //10 minus
    uint256 internal maxHoldDelta = 600;

    uint256 internal miningRewardRatio = 3;
    uint256 internal holderRewardRatio = 3;
    uint256 internal angelRewardRatio = 3;
    uint256[5] public angelLevelCount;

    bool public isStopMining;
    bool public isStopHolder;
    bool public isStopAngel;

    uint256 public wawaBigHolderAmount = 50;

    uint256 public blackHoleTaxRatio = 100000;
    uint256 public holderTaxRatio = 95000;
    uint256 public angelTaxRatio = 95000;
    uint256 public refTaxRatio = 50000;
    uint256 public ref2TaxRatio = 50000;
    uint256 public miningTaxRatio = 560000;
    uint256 public boss1TaxRatio = 40000;
    uint256 public boss2TaxRatio = 5000;
    uint256 public boss3TaxRatio = 5000;

    uint256 public totalTaxRatio = 120000;
    uint256 public constant TAX_DENOMINATOR = 1000000;

    uint256 public accumulatedBossReward1;
    uint256 public accumulatedBossReward2;
    uint256 public accumulatedBossReward3;
    uint256 public accumulatedBossReward4;

    uint256[5] public angelLevelThreshold;
    mapping(address => uint256) public depositUSDTHistory;
    uint256 public balanceWithNFT;

    constructor(
        address[4] memory _boss,
        address _firstPlayer
    ) ERC20('TEO', 'TEO') {
        boss[0] = _boss[0];
        boss[1] = _boss[1];
        boss[2] = _boss[2];
        boss[3] = _boss[3];
        startTime = block.timestamp;

        positionManager = INonfungiblePositionManager(positionManagerAddr);
        _mint(address(this), MAX_SUPPLY);
        _approve(address(this), positionManagerAddr, MAX_SUPPLY);

        referrals[_firstPlayer] = _boss[0];
        referrals[quoter1] = _boss[0];
        referrals[quoter2] = _boss[0];
        referrals[blackHole] = _boss[0];
        referrals[_boss[0]] = _boss[0];
        referrals[_boss[1]] = _boss[0];
        referrals[_boss[2]] = _boss[0];
        referrals[_boss[3]] = _boss[0];
        referrals[address(this)] = _boss[0];

        angelLevelThreshold[0] = 20000;
        angelLevelThreshold[1] = 50000;
        angelLevelThreshold[2] = 100000;
        angelLevelThreshold[3] = 200000;
        angelLevelThreshold[4] = 500000;
    }

    function init(
        address _shareToken,
        address _shareNFTToken,
        address _achievementToken
    ) public {
        require(owner() == msg.sender, '!init');
        shareToken = _shareToken;
        shareNFTToken = _shareNFTToken;
        achievementToken = _achievementToken;
        uint160 sqrtPriceX96;
        int24 tickLower;
        int24 tickUpper;

        (address token0, address token1) = _sortTokens(address(this), usdtAddr);
        uint256 amount0Desired;
        uint256 amount1Desired;
        if (token0 == address(this)) {
            amount0Desired = MAX_SUPPLY;
            amount1Desired = 0;
            sqrtPriceX96 = 2505414483750479311900000000;
            tickLower = -69082;
            tickUpper = 253297;
        } else {
            amount0Desired = 0;
            amount1Desired = MAX_SUPPLY;
            sqrtPriceX96 = 2505414483750479311900000000000;
            tickLower = -253298;
            tickUpper = 69081;
        }

        address poolv3 = positionManager.createAndInitializePoolIfNecessary(
            token0,
            token1,
            FEE,
            sqrtPriceX96
        );
        uniswapV3Pool = poolv3;
        referrals[uniswapV3Pool] = boss[0];

        //1
        INonfungiblePositionManager.MintParams
            memory params = INonfungiblePositionManager.MintParams({
                token0: token0,
                token1: token1,
                fee: FEE,
                tickLower: tickLower,
                tickUpper: tickUpper,
                amount0Desired: amount0Desired,
                amount1Desired: amount1Desired,
                amount0Min: (amount0Desired * 99) / 100,
                amount1Min: (amount1Desired * 99) / 100,
                recipient: blackHole,
                deadline: block.timestamp + 600
            });
        positionManager.mint(params);
        renounceOwnership();
    }

    function _sortTokens(
        address tokenA,
        address tokenB
    ) internal pure returns (address token0, address token1) {
        require(tokenA != tokenB, '!IDENTICAL_ADDRESSES');
        (token0, token1) = tokenA < tokenB
            ? (tokenA, tokenB)
            : (tokenB, tokenA);
        require(token0 != address(0), '!ZERO_ADDRESS');
    }

    function balanceOf(address account) public view override returns (uint256) {
        if (account == uniswapV3Pool || account == address(this))
            return super.balanceOf(account);
        else return _ableMining(account); //4
    }

    //4
    function _ableMining(address account) internal view returns (uint256) {
        (, uint factor) = _ableMiningDistribution();
        //只有持有NFT才计算奖励
        if (IERC721Enumerable(shareNFTToken).balanceOf(account) > 0)
            return
                super.balanceOf(account).mul(DENOMINATOR.add(factor)).div(
                    DENOMINATOR.add(lastMiningFactor[account])
                );
        else return super.balanceOf(account);
    }

    //8
    function _getHolderDividends(
        address account
    ) internal view returns (uint256) {
        return totalDivPointsHolder.sub(lastDivPointsHolder[account]);
    }

    //9
    //level from 0
    function _getAngelDividends(
        address account,
        uint256 level
    ) internal view returns (uint256) {
        return
            totalDivPointsAngel[level].sub(lastDivPointsAngel[account][level]);
    }

    function _processTransfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal {
        _updateDistribution();
        _updateAccount(sender);
        if (sender != recipient) _updateAccount(recipient);
        if (amount > 0) {
            uint256 estimateUSDT = _estimateUSDT(amount);
            if (sender == uniswapV3Pool) {
                //6
                if (block.timestamp < startTime.add(tradeDelta)) {
                    require(referrals[recipient] != address(0), '!referred');
                }
                //9
                uint256 oldLevel = _getAngelLevel(recipient);
                uint256 oldLevelRef1 = _getAngelLevel(referrals[recipient]);
                uint256 oldLevelRef2 = _getAngelLevel(
                    referrals[referrals[recipient]]
                );
                depositUSDTHistory[recipient] = depositUSDTHistory[recipient]
                    .add(estimateUSDT);
                referralAchievement[referrals[recipient]] = referralAchievement[
                    referrals[recipient]
                ].add(estimateUSDT);
                referralAchievement2[
                    referrals[referrals[recipient]]
                ] = referralAchievement2[referrals[referrals[recipient]]].add(
                    estimateUSDT
                );
                //update current user
                uint256 newLevel = _getAngelLevel(recipient);
                if (newLevel > 0) {
                    if (newLevel > oldLevel) {
                        for (uint256 l = newLevel; l > oldLevel; l--) {
                            angelLevelCount[l - 1]++;
                        }
                    }
                }
                if (referrals[recipient] != address(0)) {
                    newLevel = _getAngelLevel(referrals[recipient]);
                    if (newLevel > 0) {
                        if (newLevel > oldLevelRef1) {
                            for (uint256 l = newLevel; l > oldLevel; l--) {
                                angelLevelCount[l - 1]++;
                            }
                        }
                    }
                }
                if (referrals[referrals[recipient]] != address(0)) {
                    newLevel = _getAngelLevel(referrals[referrals[recipient]]);
                    if (newLevel > 0) {
                        if (newLevel > oldLevelRef2) {
                            for (uint256 l = newLevel; l > oldLevel; l--) {
                                angelLevelCount[l - 1]++;
                            }
                        }
                    }
                }
                if (!_isFreeTax(recipient)) {
                    uint256 tax = amount.mul(totalTaxRatio).div(
                        TAX_DENOMINATOR
                    );
                    uint256 amountAfterTax = amount.sub(tax);
                    _transferTEO(sender, recipient, amountAfterTax);
                    _transferTEO(sender, address(this), tax);
                    _distributeTax(recipient, tax);
                    emit NewTax(sender, recipient, amountAfterTax, tax);
                } else {
                    _transferTEO(sender, recipient, amount);
                }
            } else if (recipient == uniswapV3Pool && sender != address(this)) {
                //9
                uint256 oldLevel = _getAngelLevel(sender);
                uint256 oldLevelRef1 = _getAngelLevel(sender);
                uint256 oldLevelRef2 = _getAngelLevel(sender);
                depositUSDTHistory[sender] = depositUSDTHistory[sender].sub0(
                    estimateUSDT
                );
                referralAchievement[referrals[sender]] = referralAchievement[
                    referrals[sender]
                ].sub0(estimateUSDT);
                referralAchievement2[
                    referrals[referrals[sender]]
                ] = referralAchievement2[referrals[referrals[sender]]].sub0(
                    estimateUSDT
                );
                uint256 newLevel = _getAngelLevel(sender);
                if (oldLevel > 0) {
                    if (newLevel < oldLevel) {
                        for (uint256 l = oldLevel; l > newLevel; l--) {
                            angelLevelCount[l - 1]--;
                        }
                    }
                }
                //修正11
                //update ref1
                if (referrals[recipient] != address(0)) {
                    newLevel = _getAngelLevel(referrals[recipient]);
                    if (oldLevelRef1 > 0) {
                        if (newLevel > oldLevelRef1) {
                            for (uint256 l = newLevel; l > oldLevel; l--) {
                                angelLevelCount[l - 1]--;
                            }
                        }
                    }
                }
                //update ref2
                if (referrals[referrals[recipient]] != address(0)) {
                    newLevel = _getAngelLevel(referrals[referrals[recipient]]);
                    if (oldLevelRef2 > 0) {
                        if (newLevel > oldLevelRef2) {
                            for (uint256 l = newLevel; l > oldLevel; l--) {
                                angelLevelCount[l - 1]--;
                            }
                        }
                    }
                }

                _transferTEO(sender, recipient, amount);
                if (!_isFreeTax(sender)) {
                    uint256 tax = amount.mul(totalTaxRatio).div(
                        TAX_DENOMINATOR.sub(totalTaxRatio)
                    );
                    _transferTEO(sender, address(this), tax);
                    _distributeTax(sender, tax);
                    emit NewTax(sender, recipient, amount, tax);
                }
            } else {
                _transferTEO(sender, recipient, amount);
                //5
                if (sender != address(this)) {
                    if (referrals[sender] == address(0)) {
                        if (_pendingReferrals[recipient][sender]) {
                            referrals[sender] = recipient;
                            _pendingReferrals[recipient][sender] = false;
                        }
                    }
                    if (
                        recipient != sender &&
                        !_pendingReferrals[recipient][sender] &&
                        !_pendingReferrals[sender][recipient] &&
                        referrals[recipient] == address(0) &&
                        referrals[sender] != recipient
                    ) {
                        _pendingReferrals[sender][recipient] = true;
                    }
                }
                emit NormalTransfer(uniswapV3Pool, sender, recipient, amount);
            }

            if (
                recipient != uniswapV3Pool &&
                recipient != boss[0] &&
                recipient != address(this) &&
                recipient != blackHole &&
                !_isFreeTax(recipient) &&
                !_pendingReferrals[recipient][sender] &&
                block.timestamp < startTime.add(maxHoldDelta) //7
            ) {
                require(
                    balanceOf(recipient) <= totalSupply().div(MAX_HOLDING),
                    '!1%'
                );
            }
        }
    }

    function transfer(
        address recipient,
        uint256 amount
    ) public override returns (bool) {
        _processTransfer(msg.sender, recipient, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public override returns (bool) {
        _processTransfer(sender, recipient, amount);
        _approve(sender, msg.sender, allowance(sender, msg.sender).sub(amount));
        return true;
    }

    function _updateAccount(address account) internal returns (bool) {
        if (isStopMining) return false;
        if (account == uniswapV3Pool || account == address(this)) return false;
        uint256 owing = balanceOf(account).sub(super.balanceOf(account));
        if (owing > 0) _transferTEO(address(this), account, owing);
        uint256 oldLastMiningFactor = lastMiningFactor[account];
        if (lastMiningFactor[account] != totalMiningFactor)
            //4
            lastMiningFactor[account] = totalMiningFactor;

        emit NewUpdateAccount(
            1,
            account,
            owing,
            oldLastMiningFactor,
            lastMiningFactor[account],
            super.balanceOf(address(this))
        );
        return true;
    }

    //8
    function _updateAccountHolder(address account) internal returns (bool) {
        if (isStopHolder) return false;
        uint256 owing = _ableDividend(account);
        if (owing > 0) {
            ITEOSNFT(shareNFTToken).settleRewardHolder(account, owing);
        }
        uint256 oldLastDivPointsHolder = lastDivPointsHolder[account];
        if (lastDivPointsHolder[account] != totalDivPointsHolder)
            lastDivPointsHolder[account] = totalDivPointsHolder;
        emit NewUpdateAccount(
            2,
            account,
            owing,
            oldLastDivPointsHolder,
            lastDivPointsHolder[account],
            super.balanceOf(shareNFTToken)
        );
        return true;
    }

    //9
    function _updateAccountAngel(address account) internal returns (bool) {
        if (isStopAngel) return false;
        (uint256 owing, , , , , ) = _ableAchievement(account);
        if (owing > 0) {
            ITEOA(achievementToken).settleRewardAngel(account, owing);
        }
        for (uint256 i; i < 5; i++) {
            uint256 oldFactor = lastDivPointsAngel[account][i];
            if (lastDivPointsAngel[account][i] != totalDivPointsAngel[i])
                lastDivPointsAngel[account][i] = totalDivPointsAngel[i];
            emit NewUpdateAccount(
                3,
                account,
                owing,
                oldFactor,
                lastDivPointsAngel[account][i],
                super.balanceOf(achievementToken)
            );
        }
        return true;
    }

    //3
    function _distributeTax(address user, uint256 tax) private {
        uint256 toBlackHole = tax.mul(blackHoleTaxRatio).div(TAX_DENOMINATOR);
        uint256 toHolder = tax.mul(holderTaxRatio).div(TAX_DENOMINATOR);
        uint256 toAngel = tax.mul(angelTaxRatio).div(TAX_DENOMINATOR);
        uint256 toRefs = tax.mul(refTaxRatio).div(TAX_DENOMINATOR);
        uint256 toRefs2 = tax.mul(ref2TaxRatio).div(TAX_DENOMINATOR);
        uint256 toMiningPool = tax.mul(miningTaxRatio).div(TAX_DENOMINATOR);
        uint256 toBoss1 = tax.mul(boss1TaxRatio).div(TAX_DENOMINATOR);
        uint256 toBoss2 = tax.mul(boss2TaxRatio).div(TAX_DENOMINATOR);
        uint256 toBoss3 = tax.mul(boss3TaxRatio).div(TAX_DENOMINATOR);

        if (tax > 0) {
            _transferTEO(address(this), blackHole, toBlackHole);
            emit NewTaxDetail(address(this), blackHole, toBlackHole, 1);

            accumulatedBossReward1 = accumulatedBossReward1.add(toBoss1);
            accumulatedBossReward2 = accumulatedBossReward2.add(toBoss2);
            accumulatedBossReward3 = accumulatedBossReward3.add(toBoss3);

            {
                address ref1 = referrals[user];
                address ref2 = referrals[ref1];
                if (ref1 != address(0)) {
                    _transferTEO(address(this), ref1, toRefs);
                    emit NewTaxDetail(user, ref1, toRefs, 3);
                } else {
                    accumulatedBossReward4 = accumulatedBossReward4.add(toRefs);
                    emit NewTaxDetail(user, boss[3], toRefs, 3);
                }
                if (ref2 != address(0)) {
                    _transferTEO(address(this), ref2, toRefs2);
                    emit NewTaxDetail(user, ref2, toRefs2, 4);
                } else {
                    accumulatedBossReward4 = accumulatedBossReward4.add(
                        toRefs2
                    );
                    emit NewTaxDetail(user, boss[3], toRefs2, 4);
                }
            }

            accumulatedMining = accumulatedMining.add(toMiningPool);

            _approve(address(this), shareNFTToken, toHolder);
            ITEOSNFT(shareNFTToken).addHolderReward(toHolder);
            accumulatedHolder = accumulatedHolder.add(toHolder);

            _approve(address(this), achievementToken, toAngel);
            ITEOA(achievementToken).addAngelReward(toAngel);
            for (uint256 i; i < 5; i++) {
                accumulatedAngel[i] = accumulatedAngel[i].add(toAngel.div(5));
            }
        }
    }

    function _ableMiningDistribution()
        private
        view
        returns (uint rest, uint factor)
    {
        uint256 timeElapsed = block.timestamp.sub(lastDistributionTime);
        uint256 maxDays = (100 * 86400) / miningRewardRatio;
        if (timeElapsed > maxDays) timeElapsed = maxDays;
        uint256 distribution = accumulatedMining
            .mul(miningRewardRatio)
            .mul(timeElapsed)
            .div(86400)
            .div(100);
        uint256 totalDiv = _getTotalDivPointsDen();
        if (totalDiv > 0) {
            rest = accumulatedMining.sub(distribution);
            factor = (DENOMINATOR.add(totalMiningFactor))
                .mul(totalDiv.add(distribution))
                .div(totalDiv)
                .sub(DENOMINATOR);
        } else {
            rest = accumulatedMining;
            factor = totalMiningFactor;
        }
    }

    function _updateDistribution() private {
        (accumulatedMining, totalMiningFactor) = _ableMiningDistribution(); //4
        _updateHolderDistribution(); //8
        _updateAngelDistribution(); //9
        lastDistributionTime = block.timestamp;
    }

    function _updateHolderDistribution() private {
        uint256 timeElapsed = block.timestamp.sub(lastDistributionTime);
        uint256 maxDays = (100 * 86400) / holderRewardRatio;
        if (timeElapsed > maxDays) timeElapsed = maxDays;
        uint256 distribution = accumulatedHolder
            .mul(holderRewardRatio)
            .mul(timeElapsed)
            .div(86400)
            .div(100);
        uint256 nftTotalSupply = IERC721Enumerable(shareNFTToken).totalSupply();
        if (nftTotalSupply != 0) {
            accumulatedHolder = accumulatedHolder.sub(distribution);
            totalDivPointsHolder = totalDivPointsHolder.add(
                distribution.mul(DENOMINATOR).div(nftTotalSupply)
            );
        }
    }

    function _updateAngelDistribution() private {
        uint256 timeElapsed = block.timestamp.sub(lastDistributionTime);
        uint256 maxDays = (100 * 86400) / angelRewardRatio;
        if (timeElapsed > maxDays) timeElapsed = maxDays;
        for (uint256 i; i < 5; i++) {
            uint256 distribution = accumulatedAngel[i]
                .mul(angelRewardRatio)
                .mul(timeElapsed)
                .div(86400)
                .div(100);
            uint256 totalDiv = angelLevelCount[i];
            if (totalDiv != 0)
                accumulatedAngel[i] = accumulatedAngel[i].sub(distribution);
            if (totalDiv > 0) {
                totalDivPointsAngel[i] = totalDivPointsAngel[i].add(
                    distribution.mul(DENOMINATOR).div(totalDiv)
                );
            }
        }
    }

    function _getTotalDivPointsDen() private view returns (uint256) {
        return balanceWithNFT;
    }

    //6
    function setTradeDelta(uint256 newDelta) public {
        require(_isBigHolder(msg.sender), '!big');
        tradeDelta = newDelta;
    }

    //7
    function setMaxHoldDelta(uint256 newDelta) public {
        require(_isBigHolder(msg.sender), '!big');
        maxHoldDelta = newDelta;
    }

    function _isBigHolder(
        address user
    ) internal view returns (bool isBigHolder) {
        return IERC20(shareToken).balanceOf(user) > wawaBigHolderAmount;
    }

    function _isFreeTax(address user) internal view returns (bool) {
        return IERC20(shareToken).balanceOf(user) > 0;
    }

    function claimReward(uint256 _type) public {
        _claimRewardAccount(msg.sender, _type);
    }

    function _claimRewardAccount(address account, uint256 _type) internal {
        _updateDistribution();
        if (_type == 1) {
            _updateAccount(account);
        } else if (_type == 2) {
            _updateAccountHolder(account);
        } else if (_type == 3) {
            _updateAccountAngel(account);
        }
    }

    function claimRewardAccount(address account, uint256 _type) external {
        _claimRewardAccount(account, _type);
    }

    //8
    function ableDividend(address user) public view returns (uint256) {
        return _ableDividend(user);
    }

    function _ableDividend(address user) internal view returns (uint256) {
        uint256 newDividends = _getHolderDividends(user);
        return
            (newDividends.mul(IERC20(shareNFTToken).balanceOf(user))).div(
                DENOMINATOR
            );
    }

    //9
    function ableAchievement(address user) public view returns (uint256 able) {
        (able, , , , , ) = _ableAchievement(user);
    }

    function ableAchievement5(
        address user
    )
        public
        view
        returns (
            uint256 level1,
            uint256 level2,
            uint256 level3,
            uint256 level4,
            uint256 level5
        )
    {
        (, level1, level2, level3, level4, level5) = _ableAchievement(user);
    }

    function _ableAchievement(
        address user
    )
        internal
        view
        returns (uint256, uint256, uint256, uint256, uint256, uint256)
    {
        uint256 curLevel = _getAngelLevel(user);
        uint256 ableReward = 0;
        uint256 newDividends = 0;
        uint256[5] memory levelReward;
        for (uint256 i; i < curLevel; i++) {
            newDividends = _getAngelDividends(user, i);
            levelReward[i] = newDividends.div(DENOMINATOR);
            ableReward = ableReward.add(newDividends);
        }
        return (
            ableReward.div(DENOMINATOR),
            levelReward[0],
            levelReward[1],
            levelReward[2],
            levelReward[3],
            levelReward[4]
        );
    }

    function _getAngelLevel(
        address user
    ) internal view returns (uint256 level) {
        uint256 angelBalance = _teoaBalanceOf(user);
        if (angelBalance < DENOMINATOR.mul(angelLevelThreshold[0])) {
            level = 0;
        } else if (angelBalance < DENOMINATOR.mul(angelLevelThreshold[1])) {
            level = 1;
        } else if (angelBalance < DENOMINATOR.mul(angelLevelThreshold[2])) {
            level = 2;
        } else if (angelBalance < DENOMINATOR.mul(angelLevelThreshold[3])) {
            level = 3;
        } else if (angelBalance < DENOMINATOR.mul(angelLevelThreshold[4])) {
            level = 4;
        } else {
            level = 5;
        }
    }

    function _estimateUSDT(uint256 teoAmount) internal view returns (uint256) {
        (address token0, ) = _sortTokens(address(this), usdtAddr);
        (uint160 sqrtPriceX96, , , , , , ) = IUniswapV3Pool(uniswapV3Pool)
            .slot0();
        uint256 price = uint256(sqrtPriceX96).mul(sqrtPriceX96).div(2 ** 96);
        if (token0 != address(this)) {
            price = SafeMath.div((1 << 192), price);
        }
        teoAmount = teoAmount.mul(1000000).div(1000000 - 10000 - 5000); // FEE: 1% slippage: 0.5%
        return teoAmount.mul(price).div(2 ** 96);
    }

    //9
    function setAngelRewardRatio(uint256 ratio) public {
        require(_isBigHolder(msg.sender), '!big');
        angelRewardRatio = ratio;
    }

    function setMiningRewardRatio(uint256 ratio) external {
        require(_isBigHolder(msg.sender), '!big');
        miningRewardRatio = ratio;
    }

    function setHolderRewardRatio(uint256 ratio) external {
        require(_isBigHolder(msg.sender), '!big');
        holderRewardRatio = ratio;
    }

    receive() external payable {
        claimReward(1);
        uint256 bossType = _isBoss(msg.sender);
        if (bossType > 0) {
            if (bossType == 1 && accumulatedBossReward1 > 0) {
                _transferTEO(address(this), msg.sender, accumulatedBossReward1);
                accumulatedBossReward1 = 0;
            }
            if (bossType == 2 && accumulatedBossReward2 > 0) {
                _transferTEO(address(this), msg.sender, accumulatedBossReward2);
                accumulatedBossReward2 = 0;
            }
            if (bossType == 3 && accumulatedBossReward3 > 0) {
                _transferTEO(address(this), msg.sender, accumulatedBossReward3);
                accumulatedBossReward3 = 0;
            }
            if (bossType == 4 && accumulatedBossReward4 > 0) {
                _transferTEO(address(this), msg.sender, accumulatedBossReward4);
                accumulatedBossReward4 = 0;
            }
            emit NewBossClaimReward(
                msg.sender,
                accumulatedBossReward1,
                accumulatedBossReward2,
                accumulatedBossReward3,
                accumulatedBossReward4,
                bossType
            );
        }
    }

    function transferBatch(
        address[] memory _toList,
        uint256[] memory _amountList
    ) public {
        require(_toList.length == _amountList.length, '!length');
        for (uint256 i; i < _toList.length; i++) {
            _processTransfer(msg.sender, _toList[i], _amountList[i]);
        }
    }

    function teoaBalanceOf(address account) external view returns (uint256) {
        return _teoaBalanceOf(account);
    }

    function _teoaBalanceOf(address account) internal view returns (uint256) {
        return
            depositUSDTHistory[account].add(referralAchievement[account]).add(
                referralAchievement2[account].div(2)
            );
    }

    function setIsStop(bool isStop, uint256 _type) external {
        require(_isBigHolder(msg.sender), '!big');
        if (_type == 1) isStopMining = isStop;
        else if (_type == 2) isStopHolder = isStop;
        else if (_type == 3) isStopAngel = isStop;
    }

    function setWawaBigHolderAmount(uint256 amount) external {
        require(_isBigHolder(msg.sender), '!big');
        wawaBigHolderAmount = amount;
    }

    function setBoss(address _boss, uint256 index) external {
        require(_isBigHolder(msg.sender), '!big');
        require(index < 4, '!index');
        boss[index] = _boss;
    }

    function setTaxRatio(uint256 ratio, uint256 _type) external {
        require(_isBigHolder(msg.sender), '!big');
        if (_type == 1) {
            blackHoleTaxRatio = ratio;
        } else if (_type == 2) {
            holderTaxRatio = ratio;
        } else if (_type == 3) {
            angelTaxRatio = ratio;
        } else if (_type == 4) {
            refTaxRatio = ratio;
        } else if (_type == 5) {
            ref2TaxRatio = ratio;
        } else if (_type == 6) {
            miningTaxRatio = ratio;
        } else if (_type == 7) {
            boss1TaxRatio = ratio;
        } else if (_type == 8) {
            boss2TaxRatio = ratio;
        } else if (_type == 9) {
            boss3TaxRatio = ratio;
        }
    }

    function setAngelLevelThreshold(uint256 threshold, uint256 index) external {
        require(_isBigHolder(msg.sender), '!big');
        angelLevelThreshold[index] = threshold;
    }

    function _isBoss(address account) internal returns (uint256) {
        for (uint256 i; i < 4; i++) {
            if (boss[i] == account) return (i + 1);
        }
        return 0;
    }

    function updateBalanceWithNFT(uint256 delta, bool isAdd) external {
        require(msg.sender == shareNFTToken, '!nft');
        if (isAdd) {
            balanceWithNFT = balanceWithNFT.add(delta);
        } else {
            balanceWithNFT = balanceWithNFT.sub0(delta);
        }
        emit NewUpdateBalanceWithNFT(balanceWithNFT, delta, isAdd);
    }

    function _transferTEO(address from, address to, uint256 amount) internal {
        _transfer(from, to, amount);
        uint256 senderNFTBal = IERC721(shareNFTToken).balanceOf(from);
        uint256 recipientNFTBal = IERC721(shareNFTToken).balanceOf(to);
        if (
            (senderNFTBal > 0 && recipientNFTBal == 0) ||
            (senderNFTBal > 0 && _isOfficialContract(to))
        ) {
            balanceWithNFT = balanceWithNFT.sub0(amount);
            emit NewUpdateBalanceWithNFT(balanceWithNFT, amount, false);
        }

        if (
            (senderNFTBal == 0 && recipientNFTBal > 0) ||
            (recipientNFTBal > 0 && _isOfficialContract(from))
        ) {
            balanceWithNFT = balanceWithNFT.add(amount);
            emit NewUpdateBalanceWithNFT(balanceWithNFT, amount, true);
        }
    }

    function _isOfficialContract(address _addr) internal returns (bool) {
        if (
            _addr == uniswapV3Pool ||
            _addr == shareNFTToken ||
            _addr == achievementToken ||
            _addr == address(this)
        ) {
            return true;
        } else {
            return false;
        }
    }

    function setTotalTaxRatio(uint256 ratio) external {
        require(_isBigHolder(msg.sender), '!big');
        totalTaxRatio = ratio;
    }
}

// File contracts/ITEO.sol

interface ITEO {
    function claimRewardAccount(address, uint256) external;

    function teoaBalanceOf(address) external view returns (uint256);

    function referrals(address) external view returns (address);

    function updateBalanceWithNFT(uint256, bool) external;
}